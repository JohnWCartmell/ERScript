<all xmlns="http://www.entitymodelling.org/diagram">    
   <shape_style>
      <id>margin</id>
      <stroke>black</stroke> 
      <stroke-width>0.02</stroke-width>
      <stroke-dasharray>.025 .025</stroke-dasharray>
      <fill>#FF44FF</fill>
      <fill-opacity>0.3</fill-opacity>  
      <fill-rule>evenodd</fill-rule>
   </shape_style>
   <shape_style>
      <id>padding</id>
      <stroke>black</stroke> 
      <stroke-width>0.02</stroke-width>
      <stroke-dasharray>.025 .025</stroke-dasharray>
      <fill>#FFFF00</fill>
      <fill-opacity>0.3</fill-opacity>  
      <fill-rule> evenodd</fill-rule>
   </shape_style>
   <shape_style>
      <id>outer</id>
      <stroke>black</stroke> 
      <stroke-width>0.02</stroke-width>
      <stroke-dasharray>.025 .025</stroke-dasharray> 
      <fill>#0066FF</fill>
      <fill-opacity>0.3</fill-opacity>  
      <fill-rule>evenodd</fill-rule>
   </shape_style>
   <shape_style>
     <id>outlinedebug</id>
     <fill-opacity>0</fill-opacity>
     <stroke>black</stroke>
     <stroke-width>0.04</stroke-width>
  </shape_style>
  <shape_style>
     <id>route</id>
     <fill-opacity>0</fill-opacity>
     <stroke>blue</stroke>
     <stroke-width>0.04</stroke-width>
  </shape_style>
  <shape_style>
     <id>eteven</id>
     <fill>#FFFFCC</fill>
     <stroke>black</stroke>
     <stroke-width>1</stroke-width>
  </shape_style>
  <shape_style>
     <id>etodd</id>
     <fill>white</fill>
     <stroke>black</stroke>
     <stroke-width>1</stroke-width>
  </shape_style>
  <shape_style>
   <id>point</id>
   <stroke>red</stroke> 
   <stroke-width>1</stroke-width>
</shape_style>
<shape_style>
  <id>outline</id>
  <fill-opacity>0</fill-opacity>
  <stroke>black</stroke>
  <stroke-width>1</stroke-width>
</shape_style>
<shape_style>
  <id>tickmarks</id>
  <fill-opacity>0</fill-opacity>
  <stroke>black</stroke>
  <stroke-width>1</stroke-width>
</shape_style>

<shape_style>
  <id>offwhite</id>
  <fill>#FCFCFC</fill>
  <stroke>black</stroke>
  <stroke-width>1</stroke-width>
</shape_style>
<shape_style>
  <id>gradient</id>
  <fill>url(#topdowngrey)</fill>
</shape_style>
<shape_style>
  <id>grey</id>
  <fill>grey</fill>
</shape_style>
<shape_style>
  <id>invisible</id>
  <fill-opacity>0</fill-opacity>
  <stroke>none</stroke>
</shape_style> 

<endline_style>
   <id>testline</id>
   <stroke>black</stroke> 
   <stroke-width>1</stroke-width>
   <stroke-dasharray>.025 .025</stroke-dasharray>
   <fill>none</fill>
   <label_long_offset>0.1</label_long_offset>
   <label_lateral_offset>0.1</label_lateral_offset>
   <lateral_extent>0.1</lateral_extent>
   <long_extent>0.1</long_extent>
   <minarmlen>0.5</minarmlen>
</endline_style>

<endline_style>
   <id>standalonepath</id>
   <stroke>blue</stroke> 
   <stroke-width>0.04</stroke-width>
   <fill>none</fill>
   <label_long_offset>0.1</label_long_offset>
   <label_lateral_offset>0.1</label_lateral_offset>
   <lateral_extent>0.1</lateral_extent>
   <long_extent>0.1</long_extent>
   <minarmlen>0.5</minarmlen>
</endline_style>

 <endline_style>
    <id>solidline</id>
    <stroke>black</stroke> 
    <stroke-width>0.02</stroke-width>
    <fill>none</fill>
    <label_long_offset>0.1</label_long_offset>
    <label_lateral_offset>0.1</label_lateral_offset>
    <lateral_extent>0.1</lateral_extent>
    <long_extent>0.1</long_extent>
    <minarmlen>0.5</minarmlen>
 </endline_style>
 <endline_style>
    <id>dashedline</id>
    <stroke>black</stroke> 
    <stroke-width>0.02</stroke-width>
    <stroke-dasharray>.07 .07</stroke-dasharray>
    <fill>none</fill>
    <label_long_offset>0.1</label_long_offset>
    <label_lateral_offset>0.1</label_lateral_offset>
    <lateral_extent>0.1</lateral_extent>
    <long_extent>0.1</long_extent>
    <minarmlen>0.5</minarmlen>
 </endline_style>


<text_style>
  <id>title</id>
  <fill>black</fill>
  <font-size>20px</font-size>
  <font-weight><bold/></font-weight>
  <font-style><normal/></font-style>
  <text-decoration><underline/></text-decoration>
</text_style>

<text_style>
  <id>normal</id>
  <fill>black</fill>
  <font-size>11px</font-size>
  <font-weight><normal/></font-weight>
  <font-style><normal/></font-style>
</text_style>
<text_style>
  <id>italic</id>
  <fill>black</fill>
  <font-size>11px</font-size>
  <font-weight><normal/></font-weight>
  <font-style><italic/></font-style>
</text_style>
<text_style>
  <id>bold</id>
  <fill>black</fill>
  <font-size>11px</font-size>
  <font-weight><bold/></font-weight>
  <font-style><normal/></font-style>
</text_style>
</all>