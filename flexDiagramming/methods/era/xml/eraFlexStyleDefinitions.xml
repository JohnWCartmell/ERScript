<all xmlns="http://www.entitymodelling.org/diagram">
     <shape_style>
        <id>eteven</id>
        <fill>#FFFFCC</fill>
        <stroke>black</stroke>
        <stroke-width>1</stroke-width>
     </shape_style>
     <shape_style>
        <id>etodd</id>
        <fill>white</fill>
        <stroke>black</stroke>
        <stroke-width>1</stroke-width>
     </shape_style>
     <shape_style>
      <id>margin</id>
      <stroke>black</stroke> 
      <stroke-width>0</stroke-width>
      <stroke-dasharray>.025 .025</stroke-dasharray>
      <fill>#FF44FF</fill>
      <fill-opacity>0</fill-opacity>  
      <fill-rule>evenodd</fill-rule>
   </shape_style>
   <shape_style>
      <id>margin:hover</id>
      <stroke-width>0.02</stroke-width>
      <fill-opacity>0.3</fill-opacity>  
   </shape_style>
   <shape_style>
      <id>padding</id>
      <stroke>black</stroke> 
      <stroke-width>0</stroke-width>
      <stroke-dasharray>.025 .025</stroke-dasharray>
      <fill>#FFFF00</fill>
      <fill-opacity>0</fill-opacity>  
      <fill-rule> evenodd</fill-rule>
   </shape_style>
   <shape_style>
      <id>padding:hover</id>
      <stroke-width>0.02</stroke-width>
      <fill-opacity>0.3</fill-opacity>  
   </shape_style>

   <endline_style>
      <id>solidline</id>
      <stroke>black</stroke> 
      <stroke-width>0.02</stroke-width>
      <fill>none</fill>
      <label_long_offset>0.1</label_long_offset>
      <label_lateral_offset>0.1</label_lateral_offset>
      <lateral_extent>0.1</lateral_extent>
      <long_extent>0.1</long_extent>
      <minarmlen>0.5</minarmlen>
   </endline_style>
   <endline_style>
      <id>dashedline</id>
      <stroke>black</stroke> 
      <stroke-width>0.02</stroke-width>
      <stroke-dasharray>.07 .07</stroke-dasharray>
      <fill>none</fill>
      <label_long_offset>0.1</label_long_offset>
      <label_lateral_offset>0.1</label_lateral_offset>
      <lateral_extent>0.1</lateral_extent>
      <long_extent>0.1</long_extent>
      <minarmlen>0.5</minarmlen>
   </endline_style>

   <text_style>
     <id>title</id>
     <fill>black</fill>
     <font-size>20</font-size>
     <font-family>Arial</font-family>
     <font-weight><bold/></font-weight>
     <font-style><normal/></font-style>
     <text-decoration><underline/></text-decoration>
   </text_style>

   <text_style>
     <id>normal</id>
     <fill>black</fill>
     <font-size>11px</font-size>
     <font-weight><normal/></font-weight>
     <font-style><normal/></font-style>
   </text_style>
   <text_style>
     <id>italic</id>
     <fill>black</fill>
     <font-size>11px</font-size>
     <font-weight><normal/></font-weight>
     <font-style><italic/></font-style>
   </text_style>
   <text_style>
     <id>bold</id>
     <fill>black</fill>
     <font-size>11px</font-size>
     <font-weight><bold/></font-weight>
     <font-style><normal/></font-style>
   </text_style>
</all>