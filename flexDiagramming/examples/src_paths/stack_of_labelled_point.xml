<diagram xmlns="http://www.entitymodelling.org/diagram" 
         xmlns:diagram="http://www.entitymodelling.org/diagram"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
>

    <include>
        <filename>../../methods/test/xml/testFlexStyleDefinitions.xml</filename>
    </include>
     <default>
      <hmin>1.0</hmin>
      <wmin>2.0</wmin>
      <margin>0.15</margin>  
      <padding>0.1</padding> 
      <packing>horizontal</packing>
      <text_style>normal</text_style>
      <shape_style>outline</shape_style>
      <debug-whitespace>true</debug-whitespace>
   </default>

  <label>
    <x><relative><offset>1.0</offset></relative></x>
    <y><relative><offset>1.0</offset></relative></y>
    <text>Stack of Labelled points</text>
    <text_style>title</text_style>
  </label>

  <enclosure>
    <x><relative><offset>1.0</offset></relative></x>
    <y><relative><offset>2.0</offset></relative></y>
    <w>20</w>
    <shape_style>gradient</shape_style>
    <debug-whitespace>false</debug-whitespace>
    <margin>0.3</margin>
    <padding>0</padding> 
    <enclosure>
      <x><relative><offset>3.0</offset></relative></x>
      <y><relative><offset>1.0</offset></relative></y>
      <h>6</h>
      <w>6</w>
      <padding>0.1</padding>
      <point>
        <x><at><centre/><edge/><parent/></at></x>
        <y><at><top/><parent/></at></y>
        <label>
         <text>qqqq</text>
         <x><local>0</local></x>
         <y><local>0.2</local></y>
        </label>
       </point>
       <point>
         <x><at><centre/><edge/><parent/></at></x>
         <y><place><top/><outer/></place><at><bottom/><outer/><predecessor/></at></y>
         <label>
           <text>qqqq</text>
           <x><local>0</local></x>
           <y><local>-0.1</local></y>
         </label>
       </point>
       <point>
         <x><at><centre/><edge/><parent/></at></x>
         <y><place><top/><outer/></place><at><bottom/><outer/><predecessor/></at></y>
         <label>
            <text>qqqq</text>
            <x><place><right/><edge/></place><at><parent/></at></x>
            <y><local>-0.1</local></y>
          </label>
        </point>
        <point>
         <x><place><centre/><edge/></place><at><centre/><edge/><parent/></at></x>
         <y><place><top/><outer/></place><at><bottom/><outer/><predecessor/></at></y>
         <label>
          <padding>0.5</padding>
            <text>qqqq</text>
            <x><place><left/><outer/></place><at><parent/></at></x>
            <y><place><top/><outer/></place><at><parent/></at></y>
          </label>
        </point>
      </enclosure>
</enclosure>





</diagram>
