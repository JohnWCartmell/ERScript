   <all xmlns="http://www.entitymodelling.org/diagram">

      <shape_style>
         <id>margin</id>
         <stroke>black</stroke>
         <stroke-width>0.02</stroke-width>
         <stroke-dasharray>.025 .025</stroke-dasharray>
         <fill>#FF44FF</fill>
         <fill-opacity>0.3</fill-opacity>  
         <fill-rule>evenodd</fill-rule>
      </shape_style>
      <shape_style>
         <id>padding</id>
         <stroke>black</stroke> 
         <stroke-width>0.02</stroke-width>
         <stroke-dasharray>.025 .025</stroke-dasharray>
         <fill>#FFFF00</fill>
         <fill-opacity>0.3</fill-opacity>  
         <fill-rule> evenodd</fill-rule>
      </shape_style>
      <shape_style>
         <id>outer</id>
         <stroke>black</stroke> 
         <stroke-width>0.02</stroke-width>
         <stroke-dasharray>.025 .025</stroke-dasharray> 
         <fill>#0066FF</fill>
         <fill-opacity>0.3</fill-opacity>  
         <fill-rule>evenodd</fill-rule>
      </shape_style>
	  <shape_style>
          <id>outline</id>
          <fill-opacity>0</fill-opacity>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
      <shape_style>
          <id>outlinedebug</id>
          <fill-opacity>0</fill-opacity>
          <stroke>black</stroke>
          <stroke-width>0.04</stroke-width>
      </shape_style>
      <shape_style>
          <id>route</id>
          <fill-opacity>0</fill-opacity>
          <stroke>blue</stroke>
          <stroke-width>0.04</stroke-width>
      </shape_style>
      <shape_style>
          <id>eteven</id>
          <fill>#FFFFCC</fill>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
      <shape_style>
          <id>etodd</id>
          <fill>white</fill>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
	  <shape_style>
         <id>point</id>
         <stroke>red</stroke> 
         <stroke-width>1</stroke-width>
      </shape_style>
      <shape_style>
          <id>entity_type_outline</id>
          <fill-opacity>0</fill-opacity>
          <stroke>red</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
	  <shape_style>
          <id>group_outline</id>
          <fill-opacity>0</fill-opacity>
          <stroke>blue</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
	  <shape_style>
          <id>tickmarks</id>
          <fill-opacity>0</fill-opacity>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>

      <shape_style>
          <id>offwhite</id>
          <fill>#FCFCFC</fill>
          <stroke>black</stroke>
          <stroke-width>1</stroke-width>
      </shape_style>
      <shape_style>
          <id>gradient</id>
          <fill>url(#topdowngrey)</fill>
      </shape_style>
      <shape_style>
          <id>grey</id>
          <fill>grey</fill>
      </shape_style>
      <shape_style>
          <id>invisible</id>
          <fill-opacity>0</fill-opacity>
          <stroke>none</stroke>
      </shape_style> 
</all>