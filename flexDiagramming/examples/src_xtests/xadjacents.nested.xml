<diagram xmlns="http://www.entitymodelling.org/diagram" xmlns:diagram="http://www.entitymodelling.org/diagram" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <include>
    <filename>shared/text_style_definitions.xml</filename>
  </include>
  <include>
    <filename>shared/shape_style_definitions.xml</filename>
  </include>
  <default>
    <hmin>1.0</hmin>
    <wmin>1.0</wmin>
    <margin>0.2</margin>
    <padding>0</padding>
    <packing>horizontal</packing>
    <text_style>normal</text_style>
    <shape_style>outline</shape_style>
    <debug-whitespace>true</debug-whitespace>
  </default>
  <h>10</h>
	<margin>0.5</margin>
	<label><text>xadjacents.nested.xml</text></label>
	<label><text>A3 is placed to the right of A2 which is to the right of A1</text></label>
	<label><text>B3 is placed to the left of B2 which is placed to the left of B1</text></label>
  <enclosure>
    <id>A</id>
    <x>
      <relative>
        <offset>11</offset>
      </relative>
    </x>
    <y>
      <abs>2</abs>
    </y>
    <h>3.0</h>
    <enclosure>
      <id>A1</id>
      <label/>
      <x>
        <relative>
          <offset>0.5</offset>
        </relative>
      </x>
      <y>
        <relative><offset>1.0</offset></relative>
      </y>
      <h>1</h>
    </enclosure>
    <enclosure>
      <id>A2</id>
      <label/>
      <x>
        <at>
          <right/>
          <of>A1</of>
        </at>
      </x>
      <y>
        <relative><offset>1.1</offset></relative>
      </y>
      <h>1</h>
    </enclosure>
    <enclosure>
      <id>A3</id>
      <label/>
      <x>
        <at>
          <right/>
          <of>A2</of>
        </at>
      </x>
      <y>
        <relative><offset>1.2</offset></relative>
      </y>
      <h>1</h>
    </enclosure>
  </enclosure>
  <enclosure>
    <id>B</id>
    <x>
      <relative>
        <offset>10</offset>
      </relative>
    </x>
    <y>
      <abs>6.5</abs>
    </y>
    <h>3</h>
    <enclosure>
      <id>B1</id>
      <label/>
      <x>
        <relative>
          <offset>6.0</offset>
        </relative>
      </x>
      <y>
        <relative><offset>1.0</offset></relative>
      </y>
      <h>1</h>
    </enclosure>
    <enclosure>
      <id>B2</id>
      <label/>
      <x>
	    <place><right/></place>
        <at>
          <left/>
          <of>B1</of>
        </at>
      </x>
      <y>
        <relative><offset>1.1</offset></relative>
      </y>
      <h>1</h>
    </enclosure>
    <enclosure>
      <id>B3</id>
      <label/>
      <x>
	    <place><right/></place>
        <at>
          <left/>
          <of>B2</of>
        </at>
      </x>
      <y>
        <relative><offset>1.2</offset></relative>
      </y>
      <h>1</h>
    </enclosure>
  </enclosure>
</diagram>