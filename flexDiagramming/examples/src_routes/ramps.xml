<diagram xmlns="http://www.entitymodelling.org/diagram" xmlns:diagram="http://www.entitymodelling.org/diagram" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <include>
        <filename>../../methods/test/xml/testFlexStyleDefinitions.xml</filename>
    </include>
	<default>
		<hmin>1.0</hmin>
		<wmin>1.0</wmin>
		<margin>0.2</margin>
		<padding>0.2</padding>
		<packing>horizontal</packing>
		<text_style>normal</text_style>
		<shape_style>outline</shape_style>
		<debug-whitespace>true</debug-whitespace>
		<line_style>standalonepath</line_style>
	</default>
	<enclosure>
		<x><local>0</local></x>
		<y><abs>2.0</abs></y>
		<!-- <h>6.5</h>  -->
		<w>20</w>
		<shape_style>gradient</shape_style>
		<debug-whitespace>false</debug-whitespace>
		<margin>1.0</margin>
		<label>
			<text>Single ramp routes. </text>
			<padding>0.5</padding>
		</label>
		<enclosure>
			<id>1</id>
		    <shape_style>outer</shape_style>
			<x><at><centre/></at></x>
			<wFill/>
			<enclosure>
				<id>A1</id>
				<label/>
			</enclosure>
			<enclosure>
				<id>B1</id>
				<label/>
				<x><at><centre/></at></x>
				<y><at><predecessor/></at></y>
			</enclosure>
			<enclosure>
				<id>C1</id>
				<label/>
				<x><at><right/></at></x>
				<y><at><predecessor/></at></y>
			</enclosure>
		</enclosure>
		<enclosure>
			<id>2</id>
		    <shape_style>outer</shape_style>
			<x><at><centre/></at></x>
			<wFill/>
			<h>4.0</h>
			<enclosure>
				<id>A2</id>
				<label/>				
				<y><local>1.9</local></y>
			</enclosure>
			<enclosure>
				<id>B2</id>
				<label/>
				<x><at><centre/></at></x>
				<y><local>1.5</local></y>
				<w>3</w>
			    <h>2</h>
			</enclosure>
			<enclosure>
				<id>C2</id>
				<label/>
				<x><at><right/></at></x>
				<y><local>1.9</local></y>
			</enclosure>
		</enclosure>
		<enclosure>
			<id>3</id>
		    <shape_style>outer</shape_style>
			<x><at><centre/></at></x>
			<wFill/>
			<padding>0.5</padding>
			<margin>0.5</margin>
			<enclosure>
				<id>A3</id>
				<label/>
			</enclosure>
			<enclosure>
				<id>B3</id>
				<label/>
				<x><at><centre/></at></x>
				<y><at><predecessor/></at></y>
			</enclosure>
			<enclosure>
				<id>C3</id>
				<label/>
				<x><at><right/></at></x>
				<y><at><predecessor/></at></y>
			</enclosure>
		</enclosure>

		<route>
			<source>
					<left_side/>
					<id>B2</id>
					<annotation>link_to_a1</annotation>
			</source>
			<destination>
					<right_side/>
					<id>A1</id>
					<annotation>back_to_b2</annotation>
					<endline><marker>hollowTriangle</marker></endline>
			</destination>
			<path>
				<ramp>
			        <startarm/>
			        <endarm/>	 
			    </ramp>
			</path>
		</route>
 		<route>
			<source>
					<top_edge/>
					<id>B2</id>
					<annotation>link_to_b1</annotation>
			</source>
			<destination>
					<bottom_edge/>
					<id>B1</id>
					<annotation>back_to_b2</annotation>
					<endline><marker>hollowTriangle</marker></endline>
			</destination>
			<path>
				<ramp>
			        <startarm/>
			        <endarm/>	 
			    </ramp>
			</path>
		</route>
		<route>
			<source>
					<right_side/>
					<id>B2</id>
					<annotation>link_to_c1</annotation>
			</source>
			<destination>
					<left_side/>
					<id>C1</id>
					<annotation>back_to_b2</annotation>
					<endline><marker>hollowTriangle</marker></endline>
			</destination>
			<path>
				<ramp>
			        <startarm/>
			        <endarm/>	 
			    </ramp>
			</path>
		</route> 

		<route>
			<source>
					<left_side/>
					<id>B2</id>
					<annotation>link_to_a2</annotation>
			</source>
			<destination>
					<right_side/>
					<id>A2</id>
					<annotation>back_to_b2</annotation>
					<endline><marker>hollowTriangle</marker></endline>
			</destination>
			<path>
				<ramp>
			        <startarm/>
			        <endarm/>	 
			    </ramp>
			</path>
		</route>

		<route>
			<source>
					<right_side/>
					<id>B2</id>
					<annotation>link_to_c2</annotation>
			</source>
			<destination>
					<left_side/>
					<id>C2</id>
					<annotation>back_to_b2</annotation>
					<endline><marker>hollowTriangle</marker></endline>
			</destination>
			<path>
				<ramp>
			        <startarm/>
			        <endarm/>	 
			    </ramp>
			</path>
		</route> 
		<route>
			<source>
					<left_side/>
					<id>B2</id>
					<annotation>link_to_a3</annotation>
			</source>
			<destination>
					<right_side/>
					<id>A3</id>
					<annotation>back_to_b2</annotation>
					<endline><marker>hollowTriangle</marker></endline>
			</destination>
			<path>
				<ramp>
			        <startarm/>
			        <endarm/>	 
			    </ramp>
			</path>
		</route>
 		<route>
			<source>
					<bottom_edge/>
					<id>B2</id>
					<annotation>link_to_b3</annotation>
			</source>
			<destination>
					<top_edge/>
					<id>B3</id>
					<annotation>back_to_b2</annotation>
					<endline><marker>hollowTriangle</marker></endline>
			</destination>
			<path>
				<ramp>
			        <startarm/>
			        <endarm/>	 
			    </ramp>
			</path>
		</route>
		<route>
			<source>
					<right_side/>
					<id>B2</id>
					<annotation>link_to_c3</annotation>
			</source>
			<destination>
					<left_side/>
					<id>C3</id>
					<annotation>back_to_b2</annotation>
					<endline><marker>hollowTriangle</marker></endline>
			</destination>
			<path>
				<ramp>
			        <startarm/>
			        <endarm/>	 
			    </ramp>
			</path>
		</route> 
	</enclosure>
</diagram>