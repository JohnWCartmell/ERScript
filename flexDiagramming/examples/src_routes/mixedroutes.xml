<diagram xmlns="http://www.entitymodelling.org/diagram" xmlns:diagram="http://www.entitymodelling.org/diagram" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <include>
        <filename>../../methods/test/xml/testFlexStyleDefinitions.xml</filename>
    </include>
	<default>
		<hmin>1.0</hmin>
		<wmin>1.0</wmin>
		<margin>0.2</margin>
		<padding>0.5</padding>
		<packing>horizontal</packing>
		<text_style>normal</text_style>
		<shape_style>outline</shape_style>
		<debug-whitespace>false</debug-whitespace>
		<line_style>testline</line_style>
	</default>
	<enclosure>
		<x><local>0</local></x>
		<y><abs>2.0</abs></y>
		<!-- <h>6.5</h>  -->
		<shape_style>gradient</shape_style>
		<debug-whitespace>false</debug-whitespace>
		<margin>1.0</margin>
		<label>
			<text>Mixture of routes and uses of linestyle and endline markers.</text>
		</label>
		<label>
			<text>Includes use of secondary annotations.</text>
		</label>
		<enclosure>
			<id>A</id>
			<x><local>9</local></x>
			<label/>
			<enclosure>
				<id>A1</id>
				<x><local>1.3</local></x>
				<label/>
			</enclosure>
			<enclosure>
				<id>A2</id>
				<y><at><predecessor/></at></y>
				<label/>
			</enclosure>
			<enclosure>
				<id>A3</id>
				<y><at><bottom/><of>A2</of></at></y>
				<label/>
			</enclosure>
			<route>
				<source>
						<right_side/>
						<id>A1</id>
						<annotation>link_to_a2</annotation>
						<linestyle>solidline</linestyle>
						<endline><marker>hollowTriangle</marker></endline>
				</source>
				<destination>
						<left_side/>
						<id>A2</id>
						<annotation>a1</annotation>
						<secondaryAnnotation>(secondary)</secondaryAnnotation>
						<endline><marker>hollowTriangle</marker></endline>
				</destination>
			</route>	
			<route>
				<source>
						<right_side/>
						<id>A1</id>
						<annotation>a3</annotation>
				</source>
				<destination>
						<left_side/>
						<id>A3</id>
						<annotation>a1</annotation>
						<endline><marker>hollowTriangle</marker></endline>
				</destination>
			</route>			   
			<route>
				<source>
						<right_side>
							<deltay>0.75</deltay>
						</right_side>
						<id>A2</id>
						<annotation>a3</annotation>
						<endline><marker>hollowTriangle</marker></endline>
				</source>
				<destination>
						<right_side>
							<deltay>0.25</deltay>
						</right_side>
						<id>A3</id>
						<annotation>a1</annotation>
				</destination>
			</route>
		</enclosure>
		<enclosure>
			<id>B</id>
			<label/>	
			<x><at><right/><predecessor/></at><delta>1.0</delta></x>
			<y><at><predecessor/></at><delta>1.0</delta></y>		  
		</enclosure>
		<enclosure>
			<id>C</id>
			<label/>	
			<x><at><centre/><of>A</of></at><delta>-1</delta></x>
			<y><at><bottom/><of>A</of></at><delta>1.0</delta></y>		  
		</enclosure>
		<enclosure>
			<id>D</id>
			<label/>	
			<y><at><predecessor/></at><delta>1.0</delta></y>		  
		</enclosure>
		<route>
			<source>
					<bottom_edge/>
					<id>A</id>
					<annotation>down_to_c</annotation>
			</source>
			<destination>
					<top_edge>
						<deltax>0.5</deltax>
					</top_edge>
					<id>C</id>
					<annotation>back_to_a</annotation>
					<secondaryAnnotation>(secondary)</secondaryAnnotation>
					<endline><marker>hollowTriangle</marker></endline>

			</destination>
		</route>	
		<route>
			<source>
					<bottom_edge/>
					<id>A</id>
					<annotation>d</annotation>
					<secondaryAnnotation>(secondary)</secondaryAnnotation>
			</source>
			<destination>
					<top_edge>
						<deltax>0.5</deltax>
					</top_edge>
					<id>D</id>
					<annotation>also_back_to_A</annotation>
					<endline><marker>hollowTriangle</marker></endline>
					<linestyle>solidline</linestyle>
			</destination>
		</route>	  
		<route>
			<source>
					<right_side>
						<deltay>1.0</deltay>
					</right_side>
					<id>A</id>
					<annotation>b</annotation>
			</source>
			<destination>
					<left_side>
						<deltay>0.5</deltay>
					</left_side>
					<id>B</id>
					<annotation>a</annotation>
			</destination>
		</route>
		<route>
			<source>
					<right_side>
						<deltay>0.75</deltay>
					</right_side>
					<id>A3</id>
					<annotation>b</annotation>
					<endline><marker>hollowTriangle</marker></endline>
			</source>
			<destination>
					<left_side>
						<deltay>0.75</deltay>
					</left_side>
					<id>B</id>
					<annotation>back_to_a</annotation>
			</destination>
		</route>
	</enclosure>
</diagram>