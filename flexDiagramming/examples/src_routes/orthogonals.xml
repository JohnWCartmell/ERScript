<diagram xmlns="http://www.entitymodelling.org/diagram" xmlns:diagram="http://www.entitymodelling.org/diagram" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<include>
		<filename>../src_xtests/shared/text_style_definitions.xml</filename>
	</include>
	<include>
		<filename>../src_xtests/shared/shape_style_definitions.xml</filename>
	</include>
	<include>
		<filename>../src_xtests/shared/endline_style_definitions.xml</filename>
	</include>
	<default>
		<hmin>1.0</hmin>
		<wmin>1.0</wmin>
		<margin>0.2</margin>
		<padding>0.15</padding>
		<packing>horizontal</packing>
		<text_style>normal</text_style>
		<shape_style>outline</shape_style>
		<debug-whitespace>true</debug-whitespace>
		<end_style>testline</end_style>
	</default>
	<enclosure>
		<x><local>0</local></x>
		<y><abs>2.0</abs></y>
		<!-- <h>6.5</h>  -->
		<shape_style>gradient</shape_style>
		<debug-whitespace>false</debug-whitespace>
		<margin>1.0</margin>
		<label>
			<text>Example 1: Enclosure A has route specified:</text>
		</label>
		<label>
			<text><![CDATA[
          yyy
        ]]></text>
		</label>
		<enclosure>
			<id>A</id>
			<x><local>11</local></x>
			<label/>
		</enclosure>
		<enclosure>
			<id>B</id>
			<label/>	
			<x><at><right/><predecessor/></at><delta>1.0</delta></x>
			<y><at><predecessor/></at><delta>1.0</delta></y>		  
		</enclosure>
		<enclosure>
			<id>C</id>
			<label/>
			<x><at><centre/><of>A</of></at></x>
			<y><at><bottom/><of>A</of></at><delta>1.0</delta></y>
		</enclosure>
		<route>
			<source>
					<right_side>
						<deltay>0.5</deltay>
						<annotate_low/>
					</right_side>
					<id>A</id>
					<annotation>link_to_b</annotation>
			</source>
			<destination>
					<left_side>
						<deltay>0.3</deltay>
						<annotate_high/>
					</left_side>
					<id>B</id>
					<annotation>back_to_a</annotation>
			</destination>
			<path><ns/></path>
		</route>	
		<route>
			<source>
					<bottom_edge>
						<deltax>0.25</deltax>
						<annotate_left/>
					</bottom_edge>
					<id>A</id>
					<annotation>down_to_c</annotation>
			</source>
			<destination>
					<top_edge>
						<deltax>0.65</deltax>
						<annotate_right/>
					</top_edge>
					<id>C</id>
					<annotation>back_to_a</annotation>
			</destination>
			<path><ew/><ns/></path>  
		</route>
	</enclosure>
</diagram>