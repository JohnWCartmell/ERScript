<diagram xmlns="http://www.entitymodelling.org/diagram" 
		xmlns:diagram="http://www.entitymodelling.org/diagram"
	    xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <include>
        <filename>../../methods/test/xml/testFlexStyleDefinitions.xml</filename>
    </include>
	<default>
		<hmin>1.0</hmin>
		<wmin>1.0</wmin>
		<margin>0.2</margin>
		<padding>0.15</padding>
		<packing>horizontal</packing>
		<text_style>normal</text_style>
		<shape_style>outline</shape_style>
		<debug-whitespace>true</debug-whitespace>
		<line_style>testline</line_style>
	</default>
	<enclosure>
		<x><local>0</local></x>
		<y><abs>2.0</abs></y>
		<!-- <h>6.5</h>  -->
		<shape_style>gradient</shape_style>
		<debug-whitespace>false</debug-whitespace>
		<margin>1.0</margin>
		<label>
			<text>Enclosure A has route specified from bottom_edge of A to lhs of B.</text>
		</label>
		<enclosure>
			<id>A</id>
			<x><local>11</local></x>
			<label/>
		</enclosure>
		<enclosure>
			<id>B</id>
			<label/>	
			<x><at><right/><predecessor/></at><delta>1.0</delta></x>
			<y><at><predecessor/></at><delta>1.0</delta></y>		  
		</enclosure>
		<route>
			<source>
				<bottom_edge>
					<deltay>0.5</deltay>
					<annotate_left/>
				</bottom_edge> 
				<id>A</id>
				<annotation>link_to_b</annotation>
			</source>
			<destination>
				<left_side>
						<deltay>0.75</deltay>
						<annotate_high/>
				</left_side> 
				<id>B</id>
				<annotation>back_to_a</annotation>
			</destination>
		</route>	
	</enclosure>
</diagram>