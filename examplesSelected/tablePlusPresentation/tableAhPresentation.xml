<?xml version="1.0" encoding="UTF-8"?>
<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <presentation><diagram><deltaw>0.2</deltaw></diagram></presentation>
   <defaults><et_width>2.75</et_width><et_y_separation>1.0</et_y_separation></defaults>
   <group>
      <name>lhs</name>
      <presentation><x>0</x><y>0</y></presentation>
      <entity_type>
         <name>tablep</name>
         <presentation><x>3.5</x></presentation>
         <value>
            <name>pname</name>
            <type>string</type>
            <identifying/>
         </value>
         <reference>
            <name>subject</name>
            <cardinality>ExactlyOne</cardinality>
            <type>table</type>
            <identifying/>
            <diagram>
               <path>
               </path>
            </diagram>
         </reference>

         <composition>
            <name>rows</name>
            <cardinality>OneOrMore</cardinality>
            <type>rowp</type>
            <identifying/>
            <inverse>..</inverse>
             <pullback>
                  <type>row</type>
                  <along>
                        <component>
                           <rel>subject</rel>
                        </component>
                  </along>
                  <projection_rel>subject</projection_rel>
               </pullback>
            <diagram>
               <path>
                  <label><position>Left</position></label>
                  <inverse><label><position>Left</position><xAdjustment>-0.05</xAdjustment></label></inverse>
                  <id><label><position>Downside</position><xAdjustment>0.3</xAdjustment></label></id>
                  <destarmlen>0.5</destarmlen>
               </path>
            </diagram>
         </composition>
         <composition>
            <name>columns</name>
            <cardinality>OneOrMore</cardinality>
            <type>columnp</type>
            <identifying/>
            <inverse>..</inverse>
             <pullback>
                  <type>column</type>
                  <along>
                        <component>
                           <rel>subject</rel>
                        </component>
                  </along>
                  <projection_rel>subject</projection_rel>
               </pullback>
            <diagram>
               <path>
                  <id><label><position>Downside</position><xAdjustment>-0.3</xAdjustment></label></id>
                  <destarmlen>0.5</destarmlen>
               </path>
            </diagram>
         </composition>
      </entity_type>
      <group>
         <name>lhs row and column</name>
         <presentation><below>tablep</below></presentation>
         <entity_type>
            <name>rowp</name>
            <presentation></presentation>
            <dependency>
               <name>..</name>
               <inverse_of>rows</inverse_of>
               <type>tablep</type>
               <cardinality>ExactlyOne</cardinality>
            </dependency>
            <value>
               <name>colour</name>
               <type>string</type>
            </value>
            <reference>
               <name>subject</name>
               <cardinality>ExactlyOne</cardinality>
               <type>row</type>
               <identifying/>
               <diagonal>
                  <join>
                     <component>
                        <rel>..</rel>
                     </component>
                     <component>
                        <rel>subject</rel>
                     </component>
                  </join>
               </diagonal>
               <riser>
                  <component>
                     <rel>..</rel>
                  </component>
               </riser>
               <diagram>
                  <path>
                  </path>
               </diagram>
            </reference>
            <composition>
               <name>cells</name>
               <cardinality>OneOrMore</cardinality>
               <type>cellp</type>
               <identifying/>
               <inverse>..</inverse>
               <pullback>
                  <type>data cell</type>
                  <along>
                        <component>
                           <rel>subject</rel>
                        </component>
                  </along>
                  <projection_rel>subject</projection_rel>
               </pullback>
               <diagram>
                  <path>
                     <align>ToSrc</align>
                     <label><position>Left</position></label>
                     <inverse><label><position>Left</position><xAdjustment>-0.05</xAdjustment></label></inverse>
                  </path>
               </diagram>
            </composition>
         </entity_type>
         <entity_type>
            <name>columnp</name>
            <presentation><rightOf>rowp</rightOf><x>2.7</x><y>1.3</y></presentation>
            <dependency>
               <name>..</name>
               <type>tablep</type>
               <cardinality>ExactlyOne</cardinality>
            </dependency>
            <reference>
               <name>subject</name>
               <cardinality>ExactlyOne</cardinality>
               <type>column</type>
               <identifying/>
               <diagonal>
                  <join>
                     <component>
                        <rel>..</rel>
                     </component>
                     <component>
                        <rel>subject</rel>
                     </component>
                  </join>
               </diagonal>
               <riser>
                  <component>
                     <rel>..</rel>
                  </component>
               </riser>
               <diagram>
                  <path>
                    <destattach>0.2</destattach>
                    <align>ToDest</align>
                    <id><label><xAdjustment>-1.5</xAdjustment></label></id>
                    <scope><label><xAdjustment>-1.5</xAdjustment></label></scope>
                  </path>
               </diagram>
            </reference>
            <reference>
               <name>cells</name>
               <type>cellp</type>
               <cardinality>OneOrMore</cardinality> <!-- strikingly this cardinality EQ cardinality (../rows) -->
               <inverse>col</inverse>
            </reference>
            <value>
               <name>font</name>
               <type>string</type>
            </value>
         </entity_type>
      </group>
      <entity_type>
         <name>cellp</name>
         <presentation>
            <xl><relative><to>rowp</to></relative></xl>
            <yt><relative><d>2.9</d><to>rowp</to></relative></yt>
         </presentation>
         <dependency>
            <name>..</name>
            <type>rowp</type>
            <cardinality>ExactlyOne</cardinality>
            <inverse_of>cells</inverse_of>
         </dependency>
         <value>
            <name>border</name>
            <type>string</type>
         </value>
         <reference>
            <name>col</name>
            <type>columnp</type>
            <cardinality>ExactlyOne</cardinality>
            <identifying/>
            <inverse>cells</inverse>
            <key>
               <join>
                  <component>
                    <rel>subject</rel>
                  </component>
                  <component>
                    <rel>col</rel>
                  </component>
               </join>
            </key>
            <diagonal>
               <join>
                  <component>
                     <rel>..</rel>
                  </component>
                  <component>
                     <rel>..</rel>
                  </component>
               </join>
            </diagonal>
            <riser>
               <component>
                  <rel>..</rel>
               </component>
            </riser>
            <diagram>
               <path>
                  <srcarmlen>0.3</srcarmlen>
                  <destattach>0.7</destattach>
                  <scope><label><xAdjustment>1.0</xAdjustment></label></scope>
               </path>
            </diagram>
         </reference>
         <reference>
            <name>subject</name>
            <cardinality>ExactlyOne</cardinality>
            <type>data cell</type>
            <identifying/>
            <diagonal>
               <join>
                  <component>
                     <rel>..</rel>
                  </component>
                  <component>
                     <rel>subject</rel>
                  </component>
               </join>
            </diagonal>
            <riser>
               <component>
                  <rel>..</rel>
               </component>
            </riser>
            <diagram>
               <path>
                   <align>ToSrc</align>
               </path>
            </diagram>
         </reference>
      </entity_type>

   </group>   <!-- end lhs -->


   <group>
      <name>rhs</name>
      <presentation><x>12</x><y>0</y></presentation>
      <entity_type>
         <name>table</name>
         <presentation><ym><relative><to>tablep</to></relative></ym><x>3.5</x></presentation>
         <value>
            <name>name</name>
            <type>string</type>
            <identifying/>
         </value>
         <composition>
            <name>rows</name>
            <cardinality>OneOrMore</cardinality>
            <type>row</type>
            <identifying/>
            <inverse>..</inverse>
            <diagram>
               <path>
                  <label><position>Left</position></label>
                  <inverse><label><position>Left</position><xAdjustment>-0.05</xAdjustment></label></inverse>
                  <id><label><position>Downside</position><xAdjustment>0.3</xAdjustment></label></id>
                  <destarmlen>0.5</destarmlen>
               </path>
            </diagram>
         </composition>
         <composition>
            <name>columns</name>
            <cardinality>OneOrMore</cardinality>
            <type>column</type>
            <identifying/>
            <inverse>..</inverse>
            <diagram>
               <path>
                  <id><label><position>Downside</position><xAdjustment>-0.3</xAdjustment></label></id>
                  <destarmlen>0.5</destarmlen>
               </path>
            </diagram>
         </composition>
      </entity_type>
      <group>
         <name>rhs row and column</name>
         <presentation><below>table</below></presentation>
         <entity_type>
            <name>row</name>
            <presentation><ym><relative><to>rowp</to></relative></ym></presentation>
            <dependency>
               <name>..</name>
               <inverse_of>rows</inverse_of>
               <type>table</type>
               <cardinality>ExactlyOne</cardinality>
            </dependency>
            <value>
               <name>number</name>
               <type>integer</type>
               <identifying/>
            </value>
            <composition>
               <name>cells</name>
               <cardinality>OneOrMore</cardinality>
               <type>data cell</type>
               <identifying/>
               <inverse>..</inverse>
               <pullback>
                  <type>column</type>
                  <along>
                        <component>
                           <rel>..</rel>
                        </component>
                  </along>
                  <projection_rel>col</projection_rel>
               </pullback>
               <diagram>
                  <path>
                     <align>ToSrc</align>
                     <label><position>Left</position></label>
                     <inverse><label><position>Left</position><xAdjustment>-0.05</xAdjustment></label></inverse>
                  </path>
               </diagram>
            </composition>
         </entity_type>
         <entity_type>
            <name>column</name>
            <presentation>
                 <ym><relative><to>columnp</to></relative></ym>
                 <xl><relative><d>2.7</d><ratio>1.0</ratio><to>row</to></relative></xl>
            </presentation>
            <dependency>
               <name>..</name>
               <type>table</type>
               <cardinality>ExactlyOne</cardinality>
            </dependency>
            <reference>
               <name>cells</name>
               <type>data cell</type>
               <cardinality>OneOrMore</cardinality> <!-- strikingly this cardinality EQ cardinality (../rows) -->
               <inverse>col</inverse>
            </reference>
            <value>
               <name>number</name>
               <type>integer</type>
               <identifying/>
            </value>
         </entity_type>
      </group>
      <entity_type>
         <name>data cell</name>
         <presentation>
            <xl><relative><to>row</to></relative></xl>
            <yt><relative><d>2.9</d><to>row</to></relative></yt>
            <deltaw>0.5</deltaw>
         </presentation>
         <dependency>
            <name>..</name>
            <type>row</type>
            <cardinality>ExactlyOne</cardinality>
            <inverse_of>cells</inverse_of>
         </dependency>
         <reference>
            <name>col</name>
            <type>column</type>
            <cardinality>ExactlyOne</cardinality>
            <identifying/>
            <inverse>cells</inverse>
            <diagonal>
               <join>
                  <component>
                     <rel>..</rel>
                  </component>
                  <component>
                     <rel>..</rel>
                  </component>
               </join>
            </diagonal>
            <riser>
               <component>
                  <rel>..</rel>
               </component>
            </riser>
            <diagram>
               <path>
                  <srcarmlen>0.3</srcarmlen>
                  <destattach>0.8</destattach>
                  <scope><label><xAdjustment>1.0</xAdjustment></label></scope>
               </path>
            </diagram>
         </reference>
         <value>
            <name>content</name>
            <type>string</type>
         </value>
      </entity_type>
   </group>

</entity_model>
