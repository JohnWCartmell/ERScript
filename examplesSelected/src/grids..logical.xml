<?xml version="1.0" encoding="UTF-8"?>
<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <description>
      A further test of the code generators and a demonstator of pullbacks. Should move to separate folder and 
      as an example of elaboration.
   </description>
   <absolute>
      <name>grids</name>
      <composition>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <type>grid</type>
      </composition>
      <composition>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <type>cardinal</type>
      </composition>
   </absolute>
   <entity_type>
      <name>grid</name>
      <attribute>
         <name>name</name>
         <type>
            <string/>
         </type>
         <identifying/>
      </attribute>
      <reference>
         <name>noOfRows</name>
         <type>cardinal</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
      </reference>
      <reference>
         <name>noOfCols</name>
         <type>cardinal</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
      </reference>
      <composition>
         <name>rows</name>
         <cardinality>
            <ZeroOneOrMore/>
         </cardinality>
         <type>row</type>
         <identifying/>
         <inverse>..</inverse>
         <pullback>
            <type>ordinal</type>
            <projection_rel>index</projection_rel>
            <along>
               <component>
                  <rel>noOfRows</rel>
               </component>
            </along>
         </pullback>
      </composition>
      <composition>
         <name>columns</name>
         <cardinality>
            <ZeroOneOrMore/>
         </cardinality>
         <type>column</type>
         <identifying/>
         <inverse>..</inverse>
         <pullback>
            <type>ordinal</type>
            <projection_rel>index</projection_rel>
            <along>
               <component>
                  <rel>noOfCols</rel>
               </component>
            </along>
         </pullback>
      </composition>
   </entity_type>
   <group>
      <name>row and column</name>
      <entity_type>
         <name>row</name>
         <dependency>
            <name>..</name>
            <inverse_of>rows</inverse_of>
            <type>grid</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
         </dependency>
         <reference>
            <name>index</name>
            <type>ordinal</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <identifying/>
            <diagonal>
               <join>
                  <component>
                     <rel>..</rel>
                  </component>
                  <component>
                     <rel>noOfRows</rel>
                  </component>
               </join>
            </diagonal>
            <riser>
               <component>
                  <rel>..</rel>
               </component>
            </riser>
         </reference>
         <composition>
            <name>cells</name>
            <cardinality>
               <OneOrMore/>
            </cardinality>
            <type>cell</type>
            <identifying/>
            <inverse>..</inverse>
            <pullback>
               <type>column</type>
               <along>
                  <component>
                     <rel>..</rel>
                  </component>
               </along>
               <projection_rel>col</projection_rel>
            </pullback>
         </composition>
      </entity_type>
      <entity_type>
         <name>column</name>
         <dependency>
            <name>..</name>
            <type>grid</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
         </dependency>
         <reference>
            <name>index</name>
            <type>ordinal</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <identifying/>
            <diagonal>
               <join>
                  <component>
                     <rel>..</rel>
                  </component>
                  <component>
                     <rel>noOfCols</rel>
                  </component>
               </join>
            </diagonal>
            <riser>
               <component>
                  <rel>..</rel>
               </component>
            </riser>
         </reference>
         <reference>
            <name>cells</name>
            <type>cell</type>
            <cardinality>
               <OneOrMore/>
            </cardinality>
            <inverse>col</inverse>
         </reference>
      </entity_type>
   </group>
   <entity_type>
      <name>cell</name>
      <dependency>
         <name>..</name>
         <type>row</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <inverse_of>cells</inverse_of>
      </dependency>
      <reference>
         <name>col</name>
         <type>column</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <identifying/>
         <inverse>cells</inverse>
         <diagonal>
            <join>
               <component>
                  <rel>..</rel>
               </component>
               <component>
                  <rel>..</rel>
               </component>
            </join>
         </diagonal>
         <riser>
            <component>
               <rel>..</rel>
            </component>
         </riser>
      </reference>
   </entity_type>
   <group>
      <name>reuseable</name>
      <entity_type>
         <name>cardinal</name>
         <attribute>
            <name>zno</name>
            <type>
               <nonNegativeInteger/>
            </type>
            <identifying/>
         </attribute>
         <composition>
            <type>ordinal</type>
            <cardinality>
               <ZeroOneOrMore/>
            </cardinality>
            <inverse>..</inverse>
            <identifying/>
         </composition>
      </entity_type>
      <entity_type>
         <name>ordinal</name>
         <dependency>
            <name>..</name>
            <type>cardinal</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
         </dependency>
         <attribute>
            <name>zno</name>
            <type>
               <nonNegativeInteger/>
            </type>
            <identifying/>
         </attribute>
      </entity_type>
   </group>
</entity_model>
