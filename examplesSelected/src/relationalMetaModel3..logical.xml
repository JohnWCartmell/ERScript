<?xml version="1.0" encoding="UTF-8"?>
<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <description>
      This is a model of relational database structure. It is an interesting example because 
      only one of the four reference relationships (that from foreign key to_table) is of global scope.
   </description>
   <absolute>
      <name>relationalMetaModel</name>
      <composition>
         <name>all tables</name>
         <type>table</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <inverse>..</inverse>
      </composition>
   </absolute>
   <entity_type>
      <name>table</name>
      <attribute>
         <name>name</name>
         <type>
            <string/>
         </type>
         <identifying/>
      </attribute>
      <dependency>
         <name>..</name>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <type/>
      </dependency>
      <composition>
         <type>primary key entry</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <identifying/>
         <sequence/>
         <inverse>of</inverse>
      </composition>
      <composition>
         <type>column</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <identifying/>
         <inverse>of</inverse>
      </composition>
      <composition>
         <type>foreign key</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <identifying/>
         <inverse>of</inverse>
      </composition>
   </entity_type>
   <group>
      <name>table_children</name>
      <entity_type>
         <name>primary key entry</name>
         <!-- remove because generated by logical2physical 
         <attribute>
            <name>seq no</name>
            <type>
               <integer/>
            </type>
         </attribute> -->
         <dependency>
            <name>of</name>
            <type>table</type>
         </dependency>
         <reference>
            <name>column</name>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <identifying/>
            <type>column</type>
            <diagonal>
               <component>
                  <rel>of</rel>
               </component>
            </diagonal>
            <riser>
               <component>
                  <rel>of</rel>
               </component>
            </riser>
            <inverse>in_primary_key</inverse>
         </reference>
      </entity_type>
      <entity_type>
         <name>column</name>
         <dependency>
            <name>of</name>
            <type>table</type>
         </dependency>
         <reference>
            <name>in_primary_key</name>
            <cardinality>
               <ZeroOrOne/>
            </cardinality>
            <type>primary key entry</type>
            <inverse>column</inverse>
         </reference>
         <attribute>
            <name>name</name>
            <type>
               <string/>
            </type>
            <identifying/>
         </attribute>
      </entity_type>
      <entity_type>
         <name>foreign key</name>
         <dependency>
            <name>of</name>
            <type>table</type>
         </dependency>
         <attribute>
            <name>name</name>
            <type>
               <string/>
            </type>
            <identifying/>
         </attribute>
         <composition>
            <type>foreign key entry</type>
            <cardinality>
               <OneOrMore/>
            </cardinality>
            <identifying/>
            <inverse>partof</inverse>
         </composition>
         <reference>
            <name>to_table</name>
            <type>table</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <diagonal><!-- was the absolute -->
               <join>
                  <component>
                     <rel>of</rel>
                  </component>
                  <component>
                     <rel>..</rel>
                  </component>
               </join>
            </diagonal>
            <riser><!-- was the absolute -->
               <component>
                  <rel>..</rel>
               </component>
            </riser>
         </reference>
      </entity_type>
   </group>
   <entity_type>
      <name>foreign key entry</name>
      <dependency>
         <name>partof</name>
         <type>foreign key</type>
      </dependency>
      <reference>
         <name>from_column</name>
         <type>column</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <diagonal>
            <join>
               <component>
                  <rel>partof</rel>
               </component>
               <component>
                  <rel>of</rel>
               </component>
            </join>
         </diagonal>
         <riser>
            <component>
               <rel>of</rel>
            </component>
         </riser>
      </reference>
      <reference>
         <name>to</name>
         <type>primary key entry</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <identifying/>
         <diagonal>
            <join>
               <component>
                  <rel>partof</rel>
               </component>
               <component>
                  <rel>to_table</rel>
               </component>
            </join>
         </diagonal>
         <riser>
            <component>
               <rel>of</rel>
            </component>
         </riser>
      </reference>
   </entity_type>
</entity_model>
