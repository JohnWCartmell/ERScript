<?xml version="1.0" encoding="UTF-8"?>
<entity_model xmlns="http://www.entitymodelling.org/ERmodel"
              xmlns:diagram="http://www.entitymodelling.org/diagram">



   <absolute>  
   <!-- need this to keep absolute at beginning after consolidation -->
   </absolute>
      
   <group>
      <name>airlinerooted</name>

      <diagram:enclosure xmlns="http://www.entitymodelling.org/diagram">
         <margin>0</margin>
         <padding>0</padding>
         <shape_style>group_outline_visible</shape_style>
      </diagram:enclosure>
            <entity_type><name>airline</name>
             <!--  <diagram:enclosure xmlns="http://www.entitymodelling.org/diagram">
                  <wFill/>
              </diagram:enclosure> -->
      </entity_type>
       <group>
         <name>inairline</name>
         <diagram:enclosure xmlns="http://www.entitymodelling.org/diagram">
            <margin>0</margin>
            <padding>0</padding>
            <shape_style>group_outline_visible</shape_style>
         </diagram:enclosure>
         <!-- change of order of subtypes is significant -->
         <entity_type><name>aircraft</name>
         <diagram:enclosure xmlns="http://www.entitymodelling.org/diagram">
            <y>
               <place><middle/></place><at><middle/><of>flight</of></at>
            </y>
         </diagram:enclosure>
         </entity_type>
         <entity_type><name>seat</name></entity_type>
         <entity_type><name>airline service</name></entity_type>
         <entity_type><name>flight</name></entity_type>
         <entity_type><name>boarding pass</name></entity_type>
      </group>

   </group>  
   

   <entity_type><name>airport</name></entity_type> <!-- order is significant -->
   <entity_type><name>passenger</name>
      <diagram:enclosure xmlns="http://www.entitymodelling.org/diagram">
         <y>
            <place><middle/></place><at><middle/><of>boarding pass</of></at>
         </y>
      </diagram:enclosure>
   </entity_type>

</entity_model>