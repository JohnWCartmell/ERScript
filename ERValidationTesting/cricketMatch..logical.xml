<?xml version="1.0" encoding="UTF-8"?>
<root>
    <schema filename="../MetaModel/xml/ERA..physical.xml"/> 
<entity_model xmlns="http://www.entitymodelling.org/ERmodel" ERScriptVersion="1.6">
   <description>cricketMatch. See also inningsfsideovergplayerinContextUnconstrained, note though the scope constraints are different.
   </description>
   <absolute>
      <composition type="match*"/>
      <name>CricketMatch</name>
   </absolute>
   <entity_type name="match">
      <identifying>
         <attribute name="id" type="string"/>
      </identifying>
      <composition type="innings*"/>
      <composition type="side*"/>
   </entity_type>
   <group name="inmatch">
      <entity_type name="innings">
         <identifying>
            <context type="match"/>
            <attribute name="number" type="string"/>
         </identifying>
         <composition type="over*"/>
         <reference name="battingSide" type="side">
            <diagonal path=".."/>
            <riser path=".."/>
         </reference>
         <reference name="fieldingSide" type="side">
            <diagonal path=".."/>
            <riser path=".."/>
         </reference>
      </entity_type>
      <entity_type name="side">
         <identifying>
            <context type="match"/>
            <attribute name="name" type="string"/>
         </identifying>
         <composition type="player*"/>
      </entity_type>
   </group>
   <entity_type name="over">
      <identifying>
         <context type="innings"/>
         <attribute name="number" type="string"/>
      </identifying>
      <composition type="delivery*"/>
      <reference name="bowler" type="player">
         <diagonal path="../fieldingSide"/>
         <riser path=".."/>
      </reference>
   </entity_type>
   <entity_type name="player">
      <identifying>
         <context type="side"/>
         <attribute name="number" type="string"/>
      </identifying>
      <attribute name="name" type="string"/>
   </entity_type>
   <entity_type name="delivery">
      <identifying>
         <context type="over"/>
         <attribute name="number" type="string"/>
      </identifying>
      <reference name="facingBatter" type="player">
         <diagonal path="../../battingSide"/>
         <riser path=".."/>
      </reference>
   </entity_type>
</entity_model>
</root>
