<?xml version="1.0" encoding="UTF-8"?><entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <absolute>
      <name>test</name>
      <composition>
         <type>E</type>
         <cardinality>ExactlyOne</cardinality>
      </composition>
      <composition>
         <type>F</type>
         <cardinality>ExactlyOne</cardinality>
      </composition>
   </absolute>
   <entity_type>
     <name>A</name>
      <presentation>
         <xl><abs><d>0.1</d></abs></xl>
         <yt><abs><d>1</d></abs></yt>
      </presentation>
   </entity_type>
   <entity_type>
      <name>B</name>
      <description> No presentation specified - defaults to to left of enclosing entity type </description>
      <presentation>
           <xl><relative><to>A</to><ratio>1.0</ratio></relative></xl>
           <yt><relative><to>A</to><ratio>-1.0</ratio></relative></yt>
      </presentation>
      <entity_type>
         <name>C</name>
         <presentation>
            <x>0.3</x>
         </presentation>
      </entity_type>
      <entity_type>
         <name>D</name>
         <presentation>
            <rightOf>C</rightOf>
         </presentation>
      </entity_type>
      <entity_type>
         <name>E</name>
         <presentation>
            <rightOf>D</rightOf>
         </presentation>
      </entity_type>
   </entity_type>
   <entity_type>
        <name>F</name>
        <description>test</description>  
        <presentation><rightOf>E</rightOf></presentation>
        <entity_type>
            <name>F1</name>
        </entity_type>
        <entity_type>
            <name>F2</name>
            <presentation><below>F1</below></presentation>
        </entity_type>
   </entity_type>
   <entity_type>
        <name>G</name>
        <description> use of xl relative to F </description>
        <presentation>
                <xl><relative><to>F</to><ratio>1.0</ratio></relative></xl>
               <!--<yt><default/></yt>-->
        </presentation>
   </entity_type>
   <entity_type>
        <name>H</name>
        <description> use of xc relative to F no ratio </description>
        <presentation><xc><relative><to>F</to></relative></xc><!--<yt><default/></yt>--></presentation>
   </entity_type>
</entity_model>
