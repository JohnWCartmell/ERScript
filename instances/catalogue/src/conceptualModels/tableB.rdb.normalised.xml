<?xml version="1.0" encoding="UTF-8"?>
<entity_model xmlns="http://www.entitymodelling.org/ERmodel"
              xmlns:xs="http://www.w3.org/2001/XMLSchema"
              xmlns:era="http://www.entitymodelling.org/ERmodel"
              xmlns:er-js="http://www.entitymodelling.org/ERmodel/javascript">
   <presentation>
      <diagram>
         <deltaw>0.2</deltaw>
      </diagram>
   </presentation>
   <defaults>
      <et_width>1.5</et_width>
      <et_y_separation>1.0</et_y_separation>
   </defaults>
   <entity_type>
      <name>table</name>
      <presentation>
         <x>2.3</x>
      </presentation>
      <value>
         <name>name</name>
         <type>string</type>
         <identifying/>
      </value>
      <elementName>table</elementName>
      <parentType/>
      <composition>
         <id>R1</id>
         <name>rows</name>
         <cardinality>OneOrMore</cardinality>
         <type>row</type>
         <identifying/>
         <inverse>ofTable</inverse>
         <diagram>
            <path>
               <label>
                  <position>Left</position>
               </label>
               <inverse>
                  <label>
                     <position>Left</position>
                     <xAdjustment>-0.05</xAdjustment>
                  </label>
               </inverse>
               <id>
                  <label>
                     <position>Downside</position>
                     <xAdjustment>0.3</xAdjustment>
                  </label>
               </id>
               <destarmlen>0.5</destarmlen>
            </path>
         </diagram>
      </composition>
      <composition>
         <id>R2</id>
         <name>columns</name>
         <cardinality>OneOrMore</cardinality>
         <type>column</type>
         <inverse>ofTable</inverse>
         <diagram>
            <path>
               <id>
                  <label>
                     <position>Downside</position>
                     <xAdjustment>-0.3</xAdjustment>
                  </label>
               </id>
               <destarmlen>0.5</destarmlen>
            </path>
         </diagram>
      </composition>
      <xpath_primary_key>name</xpath_primary_key>
      <xpath_typecheck>self::table</xpath_typecheck>
   </entity_type>
   <group>
      <name>row and column</name>
      <presentation>
         <below>table</below>
      </presentation>
      <entity_type>
         <name>row</name>
         <presentation>
            <deltaw>0.3</deltaw>
         </presentation>
         <value>
            <name>table_name</name>
            <type>string</type>
            <identifying/>
            <implementationOf>
               <rel>ofTable</rel>
               <origin>name</origin>
               <destattr>name</destattr>
            </implementationOf>
         </value>
         <value>
            <name>number</name>
	           <type>integer</type>
	           <identifying/>
         </value>
         <elementName>row</elementName>
         <parentType>table</parentType>
         <dependency>
            <name>ofTable</name>
            <inverse_of>rows</inverse_of>
            <type>table</type>
            <cardinality>ExactlyOne</cardinality>
            <xpath_evaluate>..</xpath_evaluate>
         </dependency>
         <composition>
            <id>R3</id>
            <name>cells</name>
            <cardinality>OneOrMore</cardinality>
            <type>data cell</type>
            <identifying/>
            <inverse>ofRow</inverse>
            <diagram>
               <path>
                  <label>
                     <position>Left</position>
                  </label>
                  <inverse>
                     <label>
                        <position>Left</position>
                        <xAdjustment>-0.05</xAdjustment>
                     </label>
                  </inverse>
                  <destattach>0.3</destattach>
                  <destarmlen>0.6</destarmlen>
               </path>
            </diagram>
         </composition>
         <xpath_typecheck>self::row</xpath_typecheck>
         <xpath_primary_key>concat(../../name,':',table_name number)</xpath_primary_key>
      </entity_type>
      <entity_type>
         <name>column</name>
         <presentation>
            <rightOf>row</rightOf>
            <x>0.8</x>
            <deltaw>0.3</deltaw>
         </presentation>
         <value>
            <name>title</name>
	           <type>string</type>
	           <identifying/>
         </value>
         <value>
            <name>table_name</name>
            <type>string</type>
            <implementationOf>
               <rel>ofTable</rel>
               <origin>name</origin>
               <destattr>name</destattr>
            </implementationOf>
         </value>
         <elementName>column</elementName>
         <parentType>table</parentType>
         <dependency>
            <name>ofTable</name>
            <type>table</type>
            <cardinality>ExactlyOne</cardinality>
            <xpath_evaluate>..</xpath_evaluate>
         </dependency>
         <composition>
            <id>R4</id>
            <name>cells</name>
            <cardinality>OneOrMore</cardinality>
            <type>data cell</type>
            <identifying/>
            <inverse>ofCol</inverse>
            <diagram>
               <path>
                  <destattach>0.7</destattach>
                  <destarmlen>0.6</destarmlen>
               </path>
            </diagram>
         </composition>
         <xpath_primary_key>title</xpath_primary_key>
         <xpath_typecheck>self::column</xpath_typecheck>
      </entity_type>
   </group>
   <entity_type>
      <name>data cell</name>
      <presentation>
         <below>row and column</below>
         <deltaw>0.5</deltaw>
         <y>1.7</y>
      </presentation>
      <value>
         <name>row_number</name>
         <type>integer</type>
         <identifying/>
         <implementationOf>
            <rel>ofRow</rel>
            <origin>number</origin>
            <destattr>number</destattr>
         </implementationOf>
      </value>
      <value>
         <name>column_title</name>
         <type>string</type>
         <identifying/>
         <implementationOf>
            <rel>ofCol</rel>
            <origin>title</origin>
            <destattr>title</destattr>
         </implementationOf>
      </value>
      <elementName>data cell</elementName>
      <parentType>row | column</parentType>
      <dependency>
         <name>ofRow</name>
         <type>row</type>
         <cardinality>ExactlyOne</cardinality>
         <inverse_of>cells</inverse_of>
         <xpath_evaluate>..</xpath_evaluate>
      </dependency>
      <dependency>
         <name>ofCol</name>
         <type>column</type>
         <cardinality>ExactlyOne</cardinality>
         <inverse_of>cells</inverse_of>
         <xpath_evaluate>..</xpath_evaluate>
      </dependency>
      <value>
         <name>content</name>
         <type>string</type>
      </value>
      <xpath_typecheck>self::data cell</xpath_typecheck>
      <xpath_primary_key>concat(../../concat(../../name,':',table_name number) title,':',table_name row_number column_title)</xpath_primary_key>
   </entity_type>
</entity_model>
<!-- end of file: ERmodel_v1.2/instances/catalogue/src/conceptualModels/tableB.rdb.normalised.xml--> 

