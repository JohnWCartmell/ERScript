<?xml version="1.0" encoding="UTF-8"?><entity_model xmlns="http://www.entitymodelling.org/ERmodel">
<presentation><diagram><deltaw>.5</deltaw></diagram></presentation>
<entity_type>
   <name>simple_directed_graph</name>
   <presentation>
         <x>0.3</x><y>0.1</y><h>1.1</h>
         <name>Split</name>
   </presentation>
   <composition>
      <type>vertex</type>
      <cardinality>OneOrMore</cardinality>
   </composition>
</entity_type>
<entity_type>
    <name>vertex</name>
    <composition>
       <type>arc</type>
       <cardinality>ZeroOneOrMore</cardinality>
       <identifying/>
       <inverse>from</inverse>
       <diagram>
           <path>
              <align>ToSrc</align><label>
              <position>Left</position></label>
              <inverse><label><position>Left</position></label>
              </inverse>
           </path>
       </diagram>
    </composition>
    <composition>
        <cardinality>ZeroOneOrMore</cardinality>
        <identifying/>
        <type>arc</type>
       <inverse>to</inverse>
       <diagram><path><align>ToSrc</align></path></diagram>
    </composition>
    <presentation><below>simple_directed_graph</below></presentation>
</entity_type>
<entity_type>
    <name>arc</name>
    <dependency>
       <name>to</name>
       <cardinality>ExactlyOne</cardinality>
       <type>vertex</type>
    </dependency>
    <dependency>
       <name>from</name>
       <cardinality>ExactlyOne</cardinality>
       <type>vertex</type>
    </dependency>
    <presentation><below>vertex</below><!--<y>0.7</y>--></presentation>
</entity_type>
</entity_model>
