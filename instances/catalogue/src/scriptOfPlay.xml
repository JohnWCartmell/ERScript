<?xml version="1.0" encoding="UTF-8"?><entity_model xmlns="http://www.entitymodelling.org/ERmodel">
<presentation><diagram><deltah>0.3</deltah></diagram></presentation>
<absolute>
   <composition>
      <type>play</type>
      <cardinality>OneOrMore</cardinality>
   </composition>
</absolute>
<entity_type>
    <name>play</name>
    <presentation><x>2.0</x><y>1.0</y></presentation>
    <composition>
       <type>line</type>
       <cardinality>OneOrMore</cardinality>
    </composition>
    <composition>
       <type>character</type>
       <cardinality>OneOrMore</cardinality>
    </composition>
</entity_type>
<group>
   <name>playchildren</name>
   <presentation><below>play</below></presentation>
   <entity_type>
       <name>line</name>
       <reference>
          <name>assigned_to</name>
          <type>character</type>
          <cardinality>ExactlyOne</cardinality>
          <inverse>is_assigned</inverse>
          <diagram>
             <path>
                <!--
                <label><position>Upside</position></label>
                <inverse><label><position>Downside</position></label></inverse>
                -->
                <srcarmlen>0.4</srcarmlen>
             </path>
          </diagram>
       </reference>
   </entity_type>
   <entity_type>
       <name>character</name>
       <reference>
          <name>is_assigned</name>
          <type>line</type>
          <cardinality>ZeroOneOrMore</cardinality>
          <inverse>assigned_to</inverse>
       </reference>
       <presentation><rightOf>line</rightOf><x>2.0</x></presentation> <!-- was 1.6-->
   </entity_type>
</group>
</entity_model>
