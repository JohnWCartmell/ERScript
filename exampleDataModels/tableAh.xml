<?xml version="1.0" encoding="UTF-8"?>
<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
  <presentation>
    <diagram>
      <deltaw>0.5</deltaw>
    </diagram>
  </presentation>
  <defaults>
    <et_width>1.5</et_width>
    <et_y_separation>1.0</et_y_separation>
  </defaults>
  <absolute>
    <name>tableAh</name>
    <composition>
      <cardinality>
        <OneOrMore/>
      </cardinality>
      <type>table</type>
    </composition>
  </absolute>

  <entity_type>
    <name>table</name>
    <presentation>
      <x>3.0</x><y>1.3</y>
    </presentation>
    <value>
      <name>name</name>
      <type><string/></type>
      <identifying/>
    </value>
    <composition>
      <name>rows</name>
      <cardinality>
        <OneOrMore/>
      </cardinality>
      <type>row</type>
      <identifying/>
      <inverse>ofTable</inverse>
      <diagram>
        <path>
          <label>
            <position>
              <Left/>
            </position>
          </label>
          <inverse>
            <label>
              <position>
                <Left/>
              </position>
              <xAdjustment>-0.05</xAdjustment>
            </label>
          </inverse>
          <id>
            <label>
              <position>
                <Downside/>
              </position>
              <xAdjustment>0.3</xAdjustment>
            </label>
          </id>
          <destarmlen>0.5</destarmlen>
        </path>
      </diagram>
    </composition>
    <composition>
      <name>columns</name>
      <cardinality>
        <OneOrMore/>
      </cardinality>
      <type>column</type>
      <identifying/>
      <inverse>ofTable</inverse>
      <diagram>
        <path>
          <id>
            <label>
              <position>
                <Downside/>
              </position>
              <xAdjustment>-0.3</xAdjustment>
            </label>
          </id>
          <destarmlen>0.5</destarmlen>
        </path>
      </diagram>
    </composition>
  </entity_type>
  <group>
    <name>row and column</name>
    <presentation>
      <below>table</below>
    </presentation>
    <entity_type>
      <name>row</name>
      <presentation>
        <deltaw>0.3</deltaw>
      </presentation>
      <dependency>
        <name>ofTable</name>
        <inverse_of>rows</inverse_of>
        <type>table</type>
        <cardinality><ExactlyOne/></cardinality>
      </dependency>
      <value>
        <name>number</name>
        <type><integer/></type>
        <identifying/>
      </value>
      <composition>
        <name>cells</name>
        <cardinality>
          <OneOrMore/>
        </cardinality>
        <type>data cell</type>
        <identifying/>
        <inverse>ofRow</inverse>
        <pullback>
          <type>column</type>
          <along>
            <component>
              <rel>ofTable</rel>
            </component>
          </along>
          <projection_rel>col</projection_rel>
        </pullback>
        <diagram>
          <path>
            <align>
              <ToSrc/>
            </align>
            <label>
              <position>
                <Left/>
              </position>
            </label>
            <inverse>
              <label>
                <position>
                  <Left/>
                </position>
                <xAdjustment>-0.05</xAdjustment>
              </label>
            </inverse>
          </path>
        </diagram>
      </composition>
    </entity_type>
    <entity_type>
      <name>column</name>
      <presentation>
        <rightOf>row</rightOf><x>2.5</x>
        <deltaw>0.3</deltaw>
      </presentation>
      <dependency>
        <name>ofTable</name>
        <type>table</type>
        <cardinality><ExactlyOne/></cardinality>
      </dependency>
      <reference>
        <name>cells</name>
        <type>data cell</type>
        <cardinality>
          <OneOrMore/>
        </cardinality>
        <!-- strikingly this cardinality EQ cardinality (../rows) -->
        <inverse>col</inverse>
      </reference>
      <value>
        <name>number</name>
        <type><integer/></type>
        <identifying/>
      </value>
    </entity_type>
  </group>
  <entity_type>
    <name>data cell</name>
    <presentation>
      <xl>
        <relative>
          <to>row</to>
        </relative>
      </xl>
      <yt>
        <relative>
          <d>2.7</d>
          <to>row</to>
        </relative>
      </yt>
      <deltaw>0.5</deltaw>
    </presentation>
    <dependency>
      <name>ofRow</name>
      <type>row</type>
      <cardinality><ExactlyOne/></cardinality>
      <inverse_of>cells</inverse_of>
    </dependency>
    <reference>
      <name>col</name>
      <type>column</type>
      <cardinality><ExactlyOne/></cardinality>
      <identifying/>
      <inverse>cells</inverse>
      <diagonal>
        <join>
          <component>
            <rel>ofRow</rel>
          </component>
          <component>
            <rel>ofTable</rel>
          </component>
        </join>
      </diagonal>
      <riser>
        <component>
          <rel>ofTable</rel>
        </component>
      </riser>
      <diagram>
        <path>
          <srcarmlen>0.3</srcarmlen>
          <scope>
            <label>
              <xAdjustment>2.7</xAdjustment>
            </label>
          </scope>
        </path>
      </diagram>
    </reference>
    <value>
      <name>content</name>
      <type><string/></type>
    </value>
  </entity_type>
</entity_model>
