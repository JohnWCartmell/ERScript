<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <presentation>
      <diagram>
         <deltah>1.0</deltah>
      </diagram>
   </presentation>
   <description>
      An example designed to test many features of the code generators.
   </description>
   <absolute>
      <name>codeTest</name>
      <composition>
         <type>A(1)</type>
         <cardinality>OneOrMore</cardinality>
      </composition>
      <composition>
         <name>a</name>
         <type>A(2)</type>
         <cardinality>OneOrMore</cardinality>
      </composition>
   </absolute>
   <group>
       <name>lhs</name>
      <presentation>
          <x>1.6</x>
          <y>1.3</y>
      </presentation>
      <module_name>lhs</module_name>
      <entity_type>
         <name>A(1)</name>
         <value>
            <name>name</name>
            <type>string</type>
            <identifying/>
         </value>
         <value>
            <name>bool</name>
            <type>boolean</type>
         </value>
         <value>
            <name>timestamp</name>
            <type>dateTime</type>
         </value>
         <value>
            <name>text</name>
            <type>string</type>
         </value>
         <value>
            <name>real</name>
            <type>float</type>
            <optional/>
         </value>
         <choice>
            <name>colour</name>
            <from>colour</from>
            <optional/>
         </choice>
         <composition>
            <name>b1</name>
            <type>B1</type>
            <cardinality>OneOrMore</cardinality>
            <identifying/>
            <inverse>p1</inverse>
         </composition>
         <reference>
            <name>f</name>
            <type>A(2)</type>
            <cardinality>ExactlyOne</cardinality>
            <diagonal>
              <theabsolute/>
            </diagonal>
            <riser>
              <theabsolute/>
            </riser>
            <diagram>
               <path>
                  <align>ToDest</align>
                  <label>
                     <position>Upside</position>
                  </label>
                  <inverse>
                     <label>
                        <position>Downside</position>
                     </label>
                  </inverse>
                  <scope>
                     <label>
                        <xAdjustment>-0.25</xAdjustment>
                     </label>
                  </scope>
               </path>
            </diagram>
         </reference>
      </entity_type>
      <entity_type>
         <name>B1</name>
         <presentation>
            <below>A(1)</below>
            <y>2.5</y>
            <deltaw>0.3</deltaw>
         </presentation>
         <value>
            <name>name</name>
            <type>string</type>
            <identifying/>
         </value>
         <dependency>
            <name>p1</name>
            <type>A(1)</type>
            <cardinality>ExactlyOne</cardinality>
            <inverse_of>b1</inverse_of>
         </dependency>
         <reference>
            <name>g</name>
            <type>B2</type>
            <cardinality>ExactlyOne</cardinality>
            <diagonal>
               <join>
                  <component>
                     <rel>p1</rel>
                  </component>
                  <component>
                     <rel>f</rel>
                  </component>
               </join>
            </diagonal>
            <riser>
               <join>
                  <component>
                     <rel>..</rel>
                  </component>
                  <component>
                     <rel>p2</rel>
                  </component>
               </join>
            </riser>
            <diagram>
               <path>
                  <align>ToDest</align>
                  <label>
                     <position>Upside</position>
                  </label>
                  <inverse>
                     <label>
                        <position>Downside</position>
                     </label>
                  </inverse>
                  <scope>
                     <label>
                        <xAdjustment>-0.75</xAdjustment>
                     </label>
                  </scope>
               </path>
            </diagram>
         </reference>
         <entity_type>
            <name>B11(3)</name>
         </entity_type>
         <entity_type>
            <name>B12</name>
            <presentation>
               <rightOf>B11(3)</rightOf>
            </presentation>
         </entity_type>
      </entity_type>
   </group>

   <group>
      <name>rhs</name>
      <presentation>
         <rightOf>lhs</rightOf>
         <x>3.0</x>
      </presentation>
      <module_name>rhs</module_name>

      <entity_type>
         <name>A(2)</name>
         <value>
            <name>name</name>
            <type>string</type>
            <identifying/>
         </value>
         <value>
            <name>number</name>
            <type>integer</type>
            <identifying/>
         </value>
         <composition>
            <name>x</name>
            <type>X</type>
            <cardinality>OneOrMore</cardinality>
            <identifying/>
            <inverse>p2</inverse>
         </composition>
      </entity_type>
      <entity_type>
         <name>X</name>
         <value>
            <name>Xname1</name>
            <type>string</type>
            <identifying/>
         </value>
         <value>
            <name>Xname2</name>
            <type>string</type>
            <identifying/>
         </value>
         <presentation>
            <below>A(2)</below>
            <y>1.0</y>
            <deltaw>0.3</deltaw>
         </presentation>
         <dependency>
            <name>p2</name>
            <type>A(2)</type>
            <cardinality>ExactlyOne</cardinality>
            <inverse_of>x</inverse_of>
         </dependency>
         <composition>
            <name>b2</name>
            <type>B2</type>
            <cardinality>OneOrMore</cardinality>
            <identifying/>
            <inverse>..</inverse>
         </composition>
      </entity_type>
      <entity_type>
         <name>B2</name>
         <value>
            <name>B2name1</name>
            <type>string</type>
            <identifying/>
         </value>
         <value>
            <name>B2name2</name>
            <type>string</type>
            <identifying/>
         </value>
         <presentation>
            <rightOf>B1</rightOf>
            <below>X</below>
            <deltaw>0.3</deltaw>
         </presentation>
         <dependency>
            <name>..</name>
            <type>X</type>
            <cardinality>ExactlyOne</cardinality>
            <inverse_of>b2</inverse_of>
         </dependency>
         <composition>
            <name>y</name>
            <type>Y</type>
            <cardinality>ZeroOneOrMore</cardinality>
            <inverse>..</inverse>
         </composition>
      </entity_type>
      <entity_type>
         <name>Y</name>
         <value>
            <name>name</name>
            <type>string</type>
            <identifying/>
         </value>
         <presentation>
            <below>B2</below>
            <y>1.0</y>
            <deltaw>0.3</deltaw>
         </presentation>
         <dependency>
            <name>..</name>
            <type>B2</type>
            <cardinality>ExactlyOne</cardinality>
            <inverse_of>y</inverse_of>
         </dependency>
      </entity_type>
   </group>
   <enumeration_type>
       <name>colour</name>
       <enumeration_value>
           <literal>Red</literal>
       </enumeration_value>
       <enumeration_value>
           <literal>Blue</literal>
       </enumeration_value>
       <enumeration_value>
           <literal>Yellow</literal>
       </enumeration_value>
   </enumeration_type>
</entity_model>
