<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <presentation>
      <diagram>
         <deltaw>0.2</deltaw>
      </diagram>
   </presentation>

   <defaults>
      <et_width>1.5</et_width>
      <et_y_separation>1.0</et_y_separation>
   </defaults>
   <entity_type>
      <name>compound</name>
      <presentation>
         <x>2.75</x>
      </presentation>
      <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
         <name>name</name>
         <type>
            <string/>
         </type>
         <identifying/>
      </attribute>
      <composition>
	     <name>aliases</name>
         <cardinality>
            <ZeroOneOrMore/>
         </cardinality>
         <type>alias</type>
         <identifying/>
		 <inverse>for</inverse>
         <diagram>
            <path>
               <label>
                  <position>
                     <Left/>
                  </position>
               </label>
               <inverse>
                  <label>
                     <position>
                        <Left/>
                     </position>
                     <xAdjustment>-0.05</xAdjustment>
                  </label>
               </inverse>
               <id>
                  <label>
                     <position>
                        <Downside/>
                     </position>
                     <xAdjustment>0.3</xAdjustment>
                  </label>
               </id>
               <destarmlen>0.5</destarmlen>
            </path>
         </diagram>
      </composition>
      <composition>
	     <name>formula</name>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <type>occurring element</type>
         <identifying/>
         <inverse>of</inverse>
         <diagram>
            <path>
               <id>
                  <label>
                     <position>
                        <Downside/>
                     </position>
                     <xAdjustment>-0.3</xAdjustment>
                  </label>
               </id>
               <destarmlen>0.5</destarmlen>
            </path>
         </diagram>
      </composition>
   </entity_type>
   <group>
      <presentation>
         <below>compound</below>
      </presentation>
      <entity_type>
         <name>alias</name>
         <presentation>
            <deltaw>0.3</deltaw>
         </presentation>
         <dependency>
            <name>for</name>
            <type>compound</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
         </dependency>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>aliased name</name>
            <type>
               <string/>
            </type>
            <identifying/>
         </attribute>
      </entity_type>
      <entity_type>
         <name>occurring element</name>
         <presentation>
            <rightOf>alias</rightOf>
            <x>0.8</x>
            <deltaw>0.3</deltaw>
         </presentation>
         <dependency>
            <name>of</name>
            <type>compound</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
         </dependency>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>symbol</name>
            <type>
               <integer/>
            </type>
            <identifying/>
         </attribute>
		          <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>number</name>
            <type>
               <positiveInteger/>
            </type>
			<optional/>
         </attribute>

      </entity_type>
   </group>
</entity_model>
