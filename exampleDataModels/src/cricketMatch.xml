<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <presentation>
      <diagram>
         <deltah>0.5</deltah>
      </diagram>
   </presentation>
   <description>cricketMatch. See also inningsfsideovergplayerinContextUnconstrained, note though the scope constraints are different.
   </description>
   <absolute>
      <composition>
         <type>match</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
      </composition>
   </absolute>
   <entity_type>
      <name>match</name>
      <presentation>
         <x>1.2</x>
         <y>1.3</y>
         <w>4</w>
      </presentation>
      <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
         <name>id</name>
         <type>
            <string/>
         </type>
         <identifying/>
      </attribute>
      <composition>
         <!--<name>all innings</name>-->
         <type>innings</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <identifying/>
         <inverse>..</inverse>
         <diagram>
            <path>
               <label>
                  <position>
                     <Left/>
                  </position>
               </label>
            </path>
         </diagram>
      </composition>
      <composition>
         <!--<name>all side</name>-->
         <type>side</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <identifying/>
         <inverse>..</inverse>
      </composition>
   </entity_type>
   <group>
      <name>inmatch</name>
      <presentation>
         <below>match</below>
         <y>1.25</y>
      </presentation>
      <entity_type>
         <name>innings</name>
         <presentation>
            <deltaw>0.5</deltaw>
            <deltah>0.4</deltah>
         </presentation>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>number</name>
            <type>
               <string/>
            </type>
            <identifying/>
         </attribute>
         <dependency>
            <!--<name>..</name>-->
            <type>match</type>
            <!--<inverse_of>all innings</inverse_of>-->
         </dependency>
         <composition>
            <!--<name>b1</name>-->
            <type>over</type>
            <cardinality>
               <OneOrMore/>
            </cardinality>
            <identifying/>
            <inverse>p1</inverse>
         </composition>
         <reference>
            <name>battingSide</name>
            <type>side</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <diagram>
               <path>
                  <align>
                     <ToSrc/>
                  </align>
                  <label>
                     <position>
                        <Upside/>
                     </position>
                  </label>
                  <scope>
                     <label>
                        <xAdjustment>-0.4</xAdjustment>
                     </label>
                  </scope>
               </path>
            </diagram>
            <diagonal>
               <component>
                  <rel>..</rel>
               </component>
            </diagonal>
            <riser>
               <component>
                  <rel>..</rel>
               </component>
            </riser>
         </reference>
         <reference>
            <name>fieldingSide</name>
            <type>side</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <diagram>
               <path>
                  <align>
                     <ToSrc/>
                  </align>
                  <label>
                     <position>
                        <Upside/>
                     </position>
                  </label>
                  <scope>
                     <label>
                        <xAdjustment>-0.4</xAdjustment>
                     </label>
                  </scope>
               </path>
            </diagram>
            <diagonal>
               <component>
                  <rel>..</rel>
               </component>
            </diagonal>
            <riser>
               <component>
                  <rel>..</rel>
               </component>
            </riser>
         </reference>
      </entity_type>
      <entity_type>
         <name>side</name>
         <presentation>
            <rightOf>innings</rightOf>
            <x>3.0</x>
            <deltaw>0.5</deltaw>
            <deltah>0.4</deltah>
         </presentation>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>name</name>
            <type>
               <string/>
            </type>
            <identifying/>
         </attribute>
         <dependency>
            <!--<name>..</name>-->
            <type>match</type>
            <!--<inverse_of>all side</inverse_of>-->
         </dependency>
         <composition>
            <!--<name>b2</name>-->
            <type>player</type>
            <cardinality>
               <OneOrMore/>
            </cardinality>
            <identifying/>
            <inverse>p2</inverse>
         </composition>
      </entity_type>
   </group>
   <entity_type>
      <name>over</name>
      <presentation>
         <below>innings</below>
         <y>1.05</y>
         <deltaw>0.3</deltaw>
         <deltah>0.3</deltah>
      </presentation>
      <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
         <name>number</name>
         <type>
            <string/>
         </type>
         <identifying/>
      </attribute>
      <dependency>
         <!--<name>p1</name>-->
         <type>innings</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <!--<inverse_of>b1</inverse_of>-->
      </dependency>
      <composition>
         <!--<name>xxx</name>-->
         <type>delivery</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <identifying/>
         <inverse>..</inverse>
      </composition>
      <reference>
         <name>bowler</name>
         <type>player</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <diagonal>
            <join>
               <component>
                  <rel>p1</rel>
               </component>
            </join>
         </diagonal>
         <riser>
            <join>
               <component>
                  <rel>p2</rel>
               </component>
            </join>
         </riser>
         <diagram>
            <path>
               <align>
                  <ToSrc/>
               </align>
               <label>
                  <position>
                     <Upside/>
                  </position>
               </label>
               <scope>
                  <label>
                     <xAdjustment>-0.5</xAdjustment>
                  </label>
               </scope>
            </path>
         </diagram>
      </reference>
   </entity_type>
   <entity_type>
      <name>player</name>
      <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
         <name>name</name>
         <type>
            <string/>
         </type>
         <identifying/>
      </attribute>
      <presentation>
         <rightOf>over</rightOf>
         <below>side</below>
         <deltaw>0.3</deltaw>
         <deltah>0.3</deltah>
      </presentation>
      <dependency>
         <!--<name>p2</name>-->
         <type>side</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <!--<inverse_of>b2</inverse_of>-->
      </dependency>
   </entity_type>
   <entity_type>
      <name>delivery</name>
      <presentation>
         <below>over</below>
         <y>1.15</y>
         <deltaw>0.3</deltaw>
         <deltah>0.3</deltah>
      </presentation>
      <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
         <name>number</name>
         <type>
            <string/>
         </type>
         <identifying/>
      </attribute>
      <dependency>
         <!--<name>p1</name>-->
         <type>over</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <!--<inverse_of>b1</inverse_of>-->
      </dependency>
      <reference>
         <name>facingBatter</name>
         <type>player</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <diagonal>
            <join>
               <component>
                  <rel>..</rel>
               </component>
               <component>
                  <rel>p1</rel>
               </component>
            </join>
         </diagonal>
         <riser>
            <join>
               <component>
                  <rel>p2</rel>
               </component>
            </join>
         </riser>
         <diagram>
            <path>
               <align>
                  <ToSrc/>
               </align>
               <label>
                  <position>
                     <Downside/>
                  </position>
               </label>
               <scope>
                  <label>
                     <xAdjustment>-0.5</xAdjustment>
                  </label>
               </scope>
            </path>
         </diagram>
      </reference>
   </entity_type>
</entity_model>
