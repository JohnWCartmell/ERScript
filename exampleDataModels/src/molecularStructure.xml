  <entity_type>
    <name>element</name>
    <presentation>
      <rightOf>molecularStructure</rightOf>
      <x>2.5</x>
    </presentation>
    <dependency>
      <name>..</name>
      <cardinality>
        <ExactlyOne/>
      </cardinality>
    </dependency>
    <value>
      <name>symbol</name>
      <type>
        <string/>
      </type>
      <identifying/>
    </value>
    <value>
      <name>name</name>
      <type>
        <string/>
      </type>
    </value>
    <value>
      <name>atomic number</name>
      <type>
        <integer/>
      </type>
    </value>
    <value>
      <name>relative atomic mass</name>
      <type>
        <float/>
      </type>
    </value>
