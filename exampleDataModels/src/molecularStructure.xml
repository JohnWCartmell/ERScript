<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
  <defaults>
    <et_width>1.9</et_width>
  </defaults>

  <absolute>
    <composition>
      <type>molecularStructure</type>
      <cardinality>
        <OneOrMore/>
      </cardinality>
      <inverse>..</inverse>
    </composition>
    <composition>
      <type>element</type>
      <cardinality>
        <OneOrMore/>
      </cardinality>
      <inverse>..</inverse>
    </composition>
  </absolute>

  <entity_type>
    <name>molecularStructure</name>
    <presentation>
      <x>0.1</x>
      <y>1.5</y>
    </presentation>
    <dependency>
      <name>..</name>
      <cardinality>
        <ExactlyOne/>
      </cardinality>
    </dependency>
    <value>
      <name>name</name>
      <type>
        <string/>
      </type>
      <identifying/>
    </value>
    <composition>
      <type>atom</type>
      <cardinality>
        <OneOrMore/>
      </cardinality>
      <inverse>..</inverse>
    </composition>
  </entity_type>
  <entity_type>
    <name>atom</name>
    <dependency>
      <name>..</name>
      <type>molecularStructure</type>
      <cardinality>
        <ExactlyOne/>
      </cardinality>
    </dependency>
    <value>
      <name>atomNo</name>
      <type>
        <integer/>
      </type>
      <identifying/>
    </value>
    <value>
      <name>x</name>
      <type>
        <float/>
      </type>
    </value>
    <value>
      <name>y</name>
      <type>
        <float/>
      </type>
    </value>
    <!--
      <value>
        <name>element</name>
        <type>
          <string/>
        </type>
      </value>
      -->
      <!--
    <composition>
      <type>bond formed</type>
      <cardinality>
        <ZeroOneOrMore/>
      </cardinality>
      <identifying/>
      <inverse>..</inverse>
    </composition>
    -->
    <reference>
      <name>element</name>
      <type>element</type>
      <cardinality>
        <ExactlyOne/>
      </cardinality>
      <diagonal>
        <join>
          <component>
            <rel>..</rel>
          </component>
          <component>
            <rel>..</rel>
          </component>
        </join>
      </diagonal>
      <riser>
        <component>
          <rel>..</rel>
        </component>
      </riser>
      <diagram>
        <path>
          <srcarmlen>0.5</srcarmlen>
        </path>
      </diagram>
    </reference>
    <presentation>
      <below>molecularStructure</below>
    </presentation>
  </entity_type>
