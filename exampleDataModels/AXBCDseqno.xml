<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <presentation>
      <diagram>
         <deltah>0.3</deltah>
      </diagram>
   </presentation>
   <description>
       This example shows the use of an implied seqNo identifiers
       and seqNo referential attributes in the hierarchical model 
       and explicit seqNo identifiers in the relational model.
       BUG HERE CURRENTLY?
   </description>
   <absolute>
      <composition>
         <name>context</name>
         <type>context</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
      </composition>
   </absolute>
   <entity_type>
      <name>context</name>
      <presentation>
         <x>8</x>
         <y>1.3</y>
      </presentation>
      <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
         <name>name</name>
         <type>
            <string/>
         </type>
         <identifying/>
      </attribute>
      <composition>
         <name>a</name>
         <type>A</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <inverse>..</inverse>
      </composition>
      <composition>
         <name>x</name>
         <type>X</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <inverse>..</inverse>
         <identifying/>
         <sequence/>
      </composition>
   </entity_type>
   <group>
      <name>content</name>
      <presentation>
         <below>context</below>
      </presentation>
      <entity_type>
         <name>A</name>
         <presentation>
            <x>0.6</x>
            <y>0.7</y>
         </presentation>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>name</name>
            <type>
               <string/>
            </type>
            <identifying/>
         </attribute>
         <dependency>
            <name>..</name>
            <type>context</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <inverse_of>a</inverse_of>
         </dependency>
         <reference>
            <name>f</name>
            <type>B</type>
            <identifying/>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <diagonal>
               <component>
                  <rel>..</rel>
               </component>
            </diagonal>
            <riser>
               <join>
                  <component>
                     <rel>p1</rel>
                  </component>
                  <component>
                     <rel>..</rel>
                  </component>
               </join>
            </riser>
            <diagram>
               <path>
                  <srcarmlen>0.4</srcarmlen>
                  <label>
                     <position>
                        <Upside/>
                     </position>
                  </label>
                  <scope>
                     <label>
                        <xAdjustment>-1.3</xAdjustment>
                     </label>
                  </scope>
               </path>
            </diagram>
         </reference>
      </entity_type>
      <entity_type>
         <name>X</name>
         <presentation>
            <rightOf>A</rightOf>
            <x>7.0</x>
            <y>1.0</y>
         </presentation>
         <dependency>
            <name>..</name>
            <type>context</type>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <inverse_of>x</inverse_of>
         </dependency>
         <composition>
            <name>b</name>
            <type>B</type>
            <cardinality>
               <OneOrMore/>
            </cardinality>
            <inverse>p1</inverse>
         </composition>
         <composition>
            <name>c</name>
            <type>C</type>
            <cardinality>
               <OneOrMore/>
            </cardinality>
            <inverse>p2</inverse>
         </composition>
         <composition>
            <name>d</name>
            <type>D</type>
            <cardinality>
               <OneOrMore/>
            </cardinality>
            <sequence/>
            <inverse>p3</inverse>
         </composition>
      </entity_type>
      <group>
         <name>X children</name>
         <presentation>
            <below>X</below>
            <y>1.0</y>
         </presentation>
         <entity_type>
            <name>B</name>
            <presentation>
               <deltaw>0.3</deltaw>
            </presentation>
            <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
               <name>name</name>
               <type>
                  <string/>
               </type>
               <identifying/>
            </attribute>
            <dependency>
               <name>p1</name>
               <type>X</type>
               <cardinality>
                  <ExactlyOne/>
               </cardinality>
               <inverse_of>b</inverse_of>
            </dependency>
            <reference>
               <name>g</name>
               <type>C</type>
               <cardinality>
                  <ExactlyOne/>
               </cardinality>
               <identifying/>
               <diagonal>
                  <join>
                     <component>
                        <rel>p1</rel>
                     </component>
                     <component>
                        <rel>..</rel>
                     </component>
                  </join>
               </diagonal>
               <riser>
                  <join>
                     <component>
                        <rel>p2</rel>
                     </component>
                     <component>
                        <rel>..</rel>
                     </component>
                  </join>
               </riser>
               <diagram>
                  <path>
                     <align>
                        <ToSrc/>
                     </align>
                     <label>
                        <position>
                           <Upside/>
                        </position>
                     </label>
                     <inverse>
                        <label>
                           <position>
                              <Downside/>
                           </position>
                        </label>
                     </inverse>
                     <scope>
                        <label>
                           <xAdjustment>-0.9</xAdjustment>
                        </label>
                     </scope>
                  </path>
               </diagram>
            </reference>
         </entity_type>
         <entity_type>
            <name>C</name>
            <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
               <name>name</name>
               <type>
                  <string/>
               </type>
               <identifying/>
            </attribute>
            <presentation>
               <rightOf>B</rightOf>
               <x>2.0</x>
               <deltaw>0.3</deltaw>
            </presentation>
            <dependency>
               <name>p2</name>
               <type>X</type>
               <cardinality>
                  <ExactlyOne/>
               </cardinality>
               <inverse_of>c</inverse_of>
            </dependency>
            <reference>
               <name>h</name>
               <type>D</type>
               <cardinality>
                  <ExactlyOne/>
               </cardinality>
               <identifying/>
               <diagonal>
                  <component>
                     <rel>p2</rel>
                  </component>
               </diagonal>
               <riser>
                  <component>
                     <rel>p3</rel>
                  </component>
               </riser>
               <diagram>
                  <path>
                     <align>
                        <ToSrc/>
                     </align>
                     <label>
                        <position>
                           <Upside/>
                        </position>
                     </label>
                     <inverse>
                        <label>
                           <position>
                              <Downside/>
                           </position>
                        </label>
                     </inverse>
                     <scope>
                        <label>
                           <xAdjustment>-0.5</xAdjustment>
                        </label>
                     </scope>
                  </path>
               </diagram>
            </reference>
         </entity_type>
         <entity_type>
            <name>D</name>
            <presentation>
               <rightOf>C</rightOf>
               <x>2.0</x>
               <deltaw>0.3</deltaw>
            </presentation>
            <dependency>
               <name>p3</name>
               <type>X</type>
               <cardinality>
                  <ExactlyOne/>
               </cardinality>
               <inverse_of>d</inverse_of>
            </dependency>
         </entity_type>
      </group>
   </group>
</entity_model>
