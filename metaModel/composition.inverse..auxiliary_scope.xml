<?xml version="1.0" encoding="UTF-8"?>
<entity_model xmlns="http://www.entitymodelling.org/ERmodel"
              ERScriptVersion="1.6">

   <!-- <include_model filename="ERA..commonDefinitions.xml"/> -->

   <presentation>
      <diagram>
         <deltah>0.6</deltah>
         <deltaw>1.5</deltaw>
      </diagram>
   </presentation>

   <entity_type name="entity_type">
      <presentation>
         <x>2.5</x>
         <y>1.0</y>
         <shape>
            <Top/>
         </shape>
      </presentation>
      <composition type="composition*" inverse="host_entity_type">
         <diagram>
            <path>
               <inverse>
                  <label>
                     <position><Left/></position>
                  </label>
               </inverse>
            </path>
         </diagram>
      </composition> 
   </entity_type>

   <entity_type name="composition">  <!-- use trailing 'non-breaking'  space so that "composition" may appear twice over on diagram -->
      <presentation>
         <below>entity_type</below>
         <x>-1.5</x>
         <y>1.0</y>
         <deltaw>0.1</deltaw>
         <deltah>0.4</deltah>
         <shape>
            <BottomLeft/>
         </shape>
      </presentation>
      <reference  name="inverse" type="dependency">
         <diagram>
            <path>
               <srcattach>0.6</srcattach>
               <align><ToSrc/></align>
               <label>
                  <position>
                     <Downside/>
                  </position>
                  <name>
                     <NoSplit/>
                  </name>
                  <!-- <xAdjustment>0.1</xAdjustment>
                  <yAdjustment>-0.1</yAdjustment> -->
               </label>
            </path>
         </diagram>
      </reference>
   </entity_type>

   <entity_type name="dependency">
      <presentation>
         <rightOf>composition</rightOf>
         <x>3.75</x>
         <deltah>0.4</deltah>
         <deltaw>0.1</deltaw>
         <shape>
            <BottomRight/>
         </shape>
      </presentation>
      
      <reference  name="type" type="entity_type">
         <identifying/>
         <diagram>
            <path>
               <srcattach>0.3</srcattach>
               <srcarmlem>0.2</srcarmlem>
               <destattach>0.7</destattach>
               <label>
                  <position>
                     <Upside/>
                  </position>
               <!--    <xAdjustment>0.1</xAdjustment>
                  <yAdjustment>-0.1</yAdjustment> -->
               </label>
            </path>
         </diagram>
      </reference>
   </entity_type>
</entity_model>
