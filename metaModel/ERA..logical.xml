<?xml version="1.0" encoding="UTF-8"?>
<entity_model xmlns="http://www.entitymodelling.org/ERmodel"
               xmlns:era="http://www.entitymodelling.org/ERmodel"
               ERScriptVersion="1.6">
   <absolute>
      <composition type="entity_type_like*"/>
      <composition type="absolute"/>
      <composition type="xml?"/>
   </absolute>
   <xml>
      <namespace_uri>http://www.entitymodelling.org/ERmodel</namespace_uri>
   </xml>
   <entity_type name="xml">
      <composition name="attributeDefault" type="xmlStyle(2)?"/>
      <attribute name="namespace_uri" type="string"/>
   </entity_type>
   <entity_type name="ENTITY_TYPE">
      <identifying>
         <attribute name="name" type="string"/>
      </identifying>
      <entity_type name="absolute"/>
      <entity_type name="entity_type_like">
         <dependency_group/>
         <entity_type name="entity_type">
            <composition name="xmlRepresentation" type="xmlStyle(1)?"/>
         </entity_type>
         <entity_type name="group">
            <attribute name="annotation" type="string"/>
         </entity_type>
         <composition type="entity_type_like*"/>
      </entity_type>
      <composition type="dependency_group*"/>
      <composition type="Relationship*" inverse=".."/>
      <composition type="attribute*" inverse=".."/>
      <attribute name="description" type="string"/>
      <attribute name="xpath" type="string"/>
      <attribute name="module_name" type="string"/>
   </entity_type>
   <entity_type name="attribute">
      <identifying>
         <context type="ENTITY_TYPE"/>
         <attribute name="name" type="string"/>
      </identifying>
      <composition type="implementationOf?" inverse=".."/>
      <composition name="type" type="value_type"/>
      <composition type="identifying(2)?"/>
      <composition type="optional?"/>
      <composition type="deprecated?"/>
      <composition name="xmlStyle" type="xmlStyle(2)?"/><!-- this is coded all over as xmlRepresentation I think-->
      <attribute name="description" type="string"/>
   </entity_type>
   <entity_type name="dependency_group"><!--
      <composition>
         <type>presentation</type>    
         <cardinality>
            <ZeroOrOne/>
         </cardinality>
      </composition>
   --></entity_type>
   <entity_type name="Relationship">
      <identifying>
         <context type="ENTITY_TYPE"/>
         <attribute name="name" type="string"/>
      </identifying>
      <entity_type name="reference_or_dependency">
         <entity_type name="dependency">
            <reference name="inverse_of" type="composition?" inverse="inverse"/>
         </entity_type>
         <entity_type name="reference">
            <composition name="diagonal" type="navigation?"/>
            <composition name="riser" type="navigation?"/>
            <composition name="key" type="navigation?"/>
            <composition type="projection?"/>
            <attribute name="js" type="string"/>
            <attribute name="xpath_evaluate" type="string"/>
            <reference name="inverse" type="reference?" inverse="inverse">
               <diagonal path="type"/>
               <riser path=".."/>
            </reference>
         </entity_type>
      </entity_type>
      <entity_type name="constructed_relationship">
         <composition type="navigation"/>
         <reference name="inverse" type="constructed_relationship?" inverse="inverse"/>
      </entity_type>
      <entity_type name="composition">
         <composition type="transient?"/>
         <composition type="initialiser?" inverse=".."/>
         <reference name="inverse" type="dependency?" inverse="inverse_of"/>
      </entity_type>
      <composition type="sequence?"/>
      <composition type="identifying(1)?"/>
      <composition name="cardinality" type="cardinality"/>
      <reference name="type" type="ENTITY_TYPE"/>
      <attribute name="description" type="string"/>
      <attribute name="id" type="string"/>
      <attribute name="scope" type="string"/>
      <attribute name="physical_prefix" type="string"/>
   </entity_type>
   <entity_type name="sequence"/>
   <group name="righthand_children">
      <entity_type name="projection"/>
      <entity_type name="identifying(1)">
         <composition type="inherited?"/>
      </entity_type>
      <entity_type name="inherited"/>
      <entity_type name="cardinality">
         <entity_type name="ZeroOrOne"/>
         <entity_type name="ExactlyOne"/>
         <entity_type name="ZeroOneOrMore"/>
         <entity_type name="OneOrMore"/>
      </entity_type>
   </group>
   <group name="composition_children">
      <entity_type name="transient"/>
      <entity_type name="initialiser">
         <entity_type name="pullback">
            <reference name="projection_rel" type="reference?">
               <diagonal path="../type"/>
               <riser path=".."/>
            </reference>
         </entity_type>
         <entity_type name="copy">
            <composition name="riser2" type="navigation"/>
         </entity_type>
         <composition name="along" type="navigation"/>
         <reference name="type" type="ENTITY_TYPE"/>
      </entity_type>
   </group>
   <entity_type name="navigation">
      <dependency_group/>
      <entity_type name="complex">
         <composition type="component*">
            <sequence/>
         </composition>
         <entity_type name="join"/>
         <entity_type name="aggregate"/>
      </entity_type>
      <entity_type name="component">
         <reference name="rel" type="Relationship">
            <diagonal path="src"/>
            <riser path=".."/>
         </reference>
      </entity_type>
      <group name="component children">
         <entity_type name="identity"><!--
            <description>
                The identity navigation.
        </description>
     --></entity_type>
         <entity_type name="theabsolute"><!--
            <description>
                A navigation to the ansolute.
        </description>
     --></entity_type>
      </group>
      <attribute name="xpath_evaluate" type="string"/>
      <reference name="src" type="ENTITY_TYPE?"/>
      <reference name="dest" type="ENTITY_TYPE?"/>
   </entity_type>
   <group name="attribute_children">
      <entity_type name="implementationOf">
         <reference name="destattr" type="attribute">
            <diagonal path="rel/type"/>
            <riser path=".."/>
         </reference>
         <reference name="attrOfOrigin" type="attribute">
            <diagonal path="typeOfOrigin"/>
            <riser path=".."/>
         </reference>
         <reference name="typeOfOrigin" type="ENTITY_TYPE">
            <diagonal path="^"/>
            <riser path="^"/>
         </reference>
         <reference name="rel" type="reference_or_dependency">
            <diagonal path="../.."/>
            <riser path=".."/>
         </reference>
      </entity_type>
      <entity_type name="value_type">
         <entity_type name="boolean"/>
         <entity_type name="date"/>
         <entity_type name="dateTime"/>
         <entity_type name="integer"/>
         <entity_type name="float"/>
         <entity_type name="nonNegativeInteger"/>
         <entity_type name="positiveInteger"/>
         <entity_type name="string"/>
         <entity_type name="time"/>
      </entity_type>
      <entity_type name="identifying(2)"/>
      <entity_type name="optional">
         <attribute name="value" type="string"/>
      </entity_type>
      <entity_type name="deprecated"/>
   </group>
   <entity_type name="xmlStyle(2)">
      <dependency_group/>
      <entity_type name="Attribute"/>
      <entity_type name="Element(2)"/>
      <entity_type name="Anonymous(2)"/>
   </entity_type>
   <entity_type name="xmlStyle(1)">
      <entity_type name="Element(1)"/>
      <entity_type name="Anonymous(1)"/>
   </entity_type>
   <include_model filename="ERA..derived_attributes.xml"/>
</entity_model>
