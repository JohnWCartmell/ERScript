<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <defaults>
      <et_width>1.7</et_width>
      <et_height>1.2</et_height>
   </defaults>
   <entity_type>
      <name>process</name>
      <entity_type>
         <name>entity_modelling</name>
         <presentation>
            <name>
               <Split/>
            </name>
         </presentation>
      </entity_type>
   </entity_type>
   <entity_type>
      <name>entity model</name>
      <presentation>
         <xl>
            <relative>
               <to>process</to>
               <ratio>1</ratio>
               <d>1.3</d>
            </relative>
         </xl>
         <ym>
            <relative>
               <to>entity_modelling</to>
            </relative>
         </ym>
      </presentation>
      <reference>
         <type>entity_modelling</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <diagram>
            <path/>
         </diagram>
      </reference>
   </entity_type>
</entity_model>
