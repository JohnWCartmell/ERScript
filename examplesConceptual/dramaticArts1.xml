<?xml version="1.0" encoding="UTF-8"?>

<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <presentation>
      <diagram>
         <deltah>0.4</deltah>
      </diagram>
   </presentation>
   <absolute>
      <name>dramatic arts</name>
      <attribute><name>name</name><type><string/></type><identifying/></attribute>
      <composition>
         <type>playwright</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
      </composition>
      <composition>
         <type>play</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
      </composition>
      <composition>
         <type>production</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
      </composition>
      <composition>
         <type>actor</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
      </composition>
      <composition>
         <type>venue</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
      </composition>
   </absolute>
   <entity_type>
      <name>playwright</name>
      <presentation>
         <x>0.1</x>
         <y>1.5</y>
      </presentation>
      <attribute><name>name</name><type><string/></type><identifying/></attribute>
      <reference>
         <name>the_author_of</name>
         <type>play</type>
         <cardinality><ZeroOneOrMore/></cardinality>
         <inverse>written_by</inverse>
      </reference>
   </entity_type>
   <entity_type>
      <name>play</name>
      <presentation>
         <rightOf>playwright</rightOf>
         <x>1.8</x>
      </presentation>
      <attribute><name>title</name><type><string/></type><identifying/></attribute>
      <reference>
         <name>given_in</name>
         <type>play</type>
         <cardinality><ZeroOneOrMore/></cardinality>
         <inverse>of</inverse>
      </reference>
      <reference>
         <name>written_by</name>
         <type>playwright</type>
         <cardinality><ExactlyOne/></cardinality>
         <inverse>the_author_of</inverse>
         <identifying/>
         <diagram>
            <path>
               <srcattach>0.5</srcattach>
            </path>
         </diagram>
      </reference>
      <composition>
         <name>about</name>
         <type>character</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <inverse>in</inverse>
         <diagram>
            <path>
               <label>
                  <position><Right/></position>
               </label>
            </path>
         </diagram>
      </composition>
   </entity_type>

   <entity_type>
      <name>character</name>
      <presentation>
         <below>play</below>
         <y>1.0</y>
      </presentation>
      <dependency>
         <name>in</name>
         <type>play</type>
         <identifying/>
      </dependency>
      <attribute><name>name</name><type><string/></type><identifying/></attribute>
      <reference>
         <name>portrayed_by</name>
         <type>character</type>
         <cardinality>
            <ZeroOneOrMore/>
         </cardinality>
         <inverse>the_portrayal_of</inverse>
      </reference>
   </entity_type>

   <entity_type>
      <name>production</name>
      <presentation>
         <rightOf>play</rightOf>
         <x>1.7</x>
      </presentation>
      <attribute><name>season</name><type><string/></type><identifying/></attribute>
      <composition>
         <name>cast_with</name>
         <type>dramatic_role</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <inverse>in</inverse>
         <diagram>
            <path>
               <align><ToDest/></align>
               <label>
                  <position><Left/></position>
               </label>
            </path>
         </diagram>
      </composition>
      <reference>
         <name>of</name>
         <type>play</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <inverse>given_in</inverse>
         <identifying/>
         <diagram>
            <path>
               <srcattach>0.5</srcattach>
               <label><position><Upside/></position></label>
               <inverse>
                  <label><position><Upside/></position></label>
               </inverse>
            </path>
         </diagram>
      </reference>
      <reference>
         <name>at</name>
         <type>venue</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <inverse>the_location_of</inverse>
         <identifying/>
         <diagram>
            <path>
               <srcattach>0.5</srcattach>
               <label><position><Upside/></position></label>
               <inverse>
                  <label><position><Upside/></position></label>
               </inverse>
            </path>
         </diagram>
      </reference>
   </entity_type>

   <entity_type>
      <name>dramatic_role</name>
      <presentation>
         <below>production</below>
         <rightOf>character</rightOf>
         <x>0.2</x>
         <!-- <deltah>0.3</deltah> -->
         <name>
            <Split/>
         </name>
      </presentation>
      <dependency>
         <name>in</name>
         <type>production</type>
         <inverse>cast_with</inverse>
         <identifying/>
      </dependency>
      <reference>
         <name>the_portrayal_of</name>
         <type>character</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <inverse>portrayed_by</inverse>
         <identifying/>
         <diagram>
            <path>
               <align><ToDest/></align>
            </path>
         </diagram>
         <riser>
            <component>
               <rel>in</rel>
            </component>
         </riser>
         <diagonal>
            <join>
               <component>
                  <rel>in</rel>
               </component>
               <component>
                  <rel>of</rel>
               </component>
            </join>
         </diagonal>
         <diagram>
            <path>
               <label>
                  <position>
                     <Upside/>
                  </position>
               </label>
               <inverse>
                  <label>
                     <position>
                        <Downside/>
                     </position>
                  </label>
               </inverse>
               <scope>
                  <label>
                     <xAdjustment>-0.5</xAdjustment>
                  </label>
               </scope>
            </path>
         </diagram>
      </reference>
      <reference>
         <name>played_by</name>
         <type>actor</type>
         <cardinality><ExactlyOne/></cardinality>
         <inverse>the_player_of</inverse>
         <diagram>
            <path>
               <destattach>0.5</destattach>
               <align><ToDest/></align>
            </path>
         </diagram>
      </reference>
   </entity_type>

   <entity_type>
      <name>venue</name>
      <presentation>
         <rightOf>production</rightOf>
         <x>1.65</x>
      </presentation>
      <attribute><name>name</name><type><string/></type><identifying/></attribute>
      <reference>
         <name>the_location_of</name>
         <type>production</type>
         <cardinality><ZeroOneOrMore/></cardinality>
         <inverse>at</inverse>
      </reference>
   </entity_type>

   <entity_type>
      <name>actor</name>
      <presentation>
         <rightOf>dramatic_role</rightOf>
         <x>2.4</x>
      </presentation>
      <attribute><name>name</name><type><string/></type><identifying/></attribute>
      <reference>
         <name>the_player_of</name>
         <type>dramatic_role</type>
         <cardinality><ZeroOneOrMore/></cardinality>
         <inverse>played_by</inverse>
      </reference>
   </entity_type>
</entity_model>
