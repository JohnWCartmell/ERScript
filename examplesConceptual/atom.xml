<?xml version="1.0" encoding="UTF-8"?>
<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <defaults>
      <et_width>1.8</et_width>
      <et_y_separation>0.6</et_y_separation>
   </defaults>
   <entity_type>
      <name>atom</name>
      <presentation>
         <x>2.3</x>
         <y>0.1</y>
      </presentation>
      <composition>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <type>nucleus</type>
      </composition>
      <composition>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <type>electron</type>
      </composition>
   </entity_type>
   <group>
      <name>atom_children</name>
      <presentation>
         <below>atom</below>
      </presentation>
      <entity_type>
         <name>nucleus</name>
         <composition>
            <cardinality>
               <OneOrMore/>
            </cardinality>
            <type>proton</type>
         </composition>
         <composition>
            <cardinality>
               <ZeroOneOrMore/>
            </cardinality>
            <type>neutron</type>
         </composition>
      </entity_type>
      <entity_type>
         <name>electron</name>
         <presentation>
            <rightOf>nucleus</rightOf>
         </presentation>
      </entity_type>
   </group>
   <group>
      <name>nucleons</name>
      <presentation>
         <below>nucleus</below>
      </presentation>
      <entity_type>
         <name>proton</name>
      </entity_type>
      <entity_type>
         <name>neutron</name>
         <presentation>
            <rightOf>proton</rightOf>
         </presentation>
      </entity_type>
   </group>
</entity_model>
