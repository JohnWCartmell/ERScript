<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <defaults>
      <et_width>1.4</et_width>
      <et_y_separation>0.7</et_y_separation>
   </defaults>
   <entity_type>
      <name>data table</name>
      <presentation>
         <x>1.3</x>
      </presentation>
      <composition>
         <cardinality>OneOrMore</cardinality>
         <sequence/>
         <type>row</type>
         <diagram>
            <path>
               <destarmlen>0.5</destarmlen>
            </path>
         </diagram>
      </composition>
      <composition>
         <cardinality>OneOrMore</cardinality>
         <sequence/>
         <type>column</type>
         <diagram>
            <path>
               <destarmlen>0.5</destarmlen>
            </path>
         </diagram>
      </composition>
   </entity_type>
   <group>
      <name>row and column</name>
      <presentation>
         <below>data table</below>
      </presentation>
      <entity_type>
         <name>row</name>
         <composition>
            <cardinality>OneOrMore</cardinality>
            <sequence/>
            <type>data cell</type>
            <identifying/>
            <diagram>
               <path>
                  <destattach>0.3</destattach>
                  <destarmlen>0.6</destarmlen>
               </path>
            </diagram>
         </composition>
      </entity_type>
      <entity_type>
         <name>column</name>
         <composition>
            <cardinality>OneOrMore</cardinality>
            <sequence/>
            <type>data cell</type>
            <identifying/>
            <diagram>
               <path>
                  <destattach>0.7</destattach>
                  <destarmlen>0.6</destarmlen>
               </path>
            </diagram>
         </composition>
         <presentation>
            <rightOf>row</rightOf>
            <x>0.8</x>
         </presentation>
      </entity_type>
   </group>
   <entity_type>
      <name>data cell</name>
      <presentation>
         <below>row and column</below>
         <y>1.2</y>
      </presentation>
   </entity_type>
</entity_model>
