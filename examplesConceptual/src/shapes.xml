<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <entity_type>
      <name>Shape</name>
      <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
         <name>fill colour</name>
         <type>
            <string/>
         </type>
      </attribute>
      <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
         <name>line thickness</name>
         <type>
            <string/>
         </type>
      </attribute>
      <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
         <name>position</name>
         <type>
            <string/>
         </type>
      </attribute>
      <entity_type>
         <name>regular polygon</name>
         <presentation>
            <w>2.5</w>
         </presentation>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>no of sides</name>
            <type>
               <string/>
            </type>
         </attribute>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>length of side</name>
            <type>
               <string/>
            </type>
         </attribute>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>orientation</name>
            <type>
               <string/>
            </type>
         </attribute>
      </entity_type>
      <entity_type>
         <name>circle</name>
         <presentation>
            <rightOf>regular polygon</rightOf>
         </presentation>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>radius</name>
            <type>
               <string/>
            </type>
         </attribute>
      </entity_type>
      <entity_type>
         <name>ellipse</name>
         <presentation>
            <rightOf>circle</rightOf>
            <w>2.5</w>
         </presentation>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>major axis</name>
            <type>
               <string/>
            </type>
         </attribute>
         <attribute xmlns:era="http://www.entitymodelling.org/ERmodel">
            <name>minor axis</name>
            <type>
               <string/>
            </type>
         </attribute>
      </entity_type>
   </entity_type>
</entity_model>
