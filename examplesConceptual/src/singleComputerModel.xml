<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <absolute>
      <name>single computer&gt;</name>
      <composition>
         <type>cpu</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
      </composition>
      <composition>
         <type>memory</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
      </composition>
      <composition>
         <type>drive</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
      </composition>
   </absolute>
   <entity_type>
      <name>cpu</name>
      <presentation>
         <x>0.3</x>
         <y>1.0</y>
      </presentation>
   </entity_type>
   <entity_type>
      <name>memory</name>
      <presentation>
         <rightOf>cpu</rightOf>
      </presentation>
   </entity_type>
   <entity_type>
      <name>drive</name>
      <presentation>
         <rightOf>memory</rightOf>
      </presentation>
      <composition>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <type>folder</type>
      </composition>
   </entity_type>
   <entity_type>
      <name>folder</name>
      <presentation>
         <below>drive</below>
      </presentation>
   </entity_type>
</entity_model>
