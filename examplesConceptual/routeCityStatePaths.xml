<?xml version="1.0" encoding="UTF-8"?>

<entity_model xmlns="http://www.entitymodelling.org/ERmodel">
   <defaults>
      <et_width>1.4</et_width>
      <et_y_separation>0.7</et_y_separation>
   </defaults>
   <presentation>
      <diagram>
         <deltaw>0.5</deltaw>
         <relid_condition><All/></relid_condition>
      </diagram>
   </presentation>
   <entity_type>
      <name>state</name>
      <presentation>
         <x>1.6</x>
      </presentation>
      <composition>
         <name>location_of</name>
         <type>city</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <inverse>located_within</inverse>
         <identifying/>
         <id>d1</id>
         <diagram>
            <path>
               <destarmlen>0.5</destarmlen>
               <label>
                     <name><None/></name>
               </label>
               <inverse>
                  <label>
                     <position>
                        <Left/>
                     </position>
                  </label>
               </inverse>
            </path>
         </diagram>
      </composition>
      <composition>
         <name>location_of </name>
         <type>city </type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <inverse>located_within</inverse>
         <identifying/>
         <id>d1</id>
         <diagram>
            <path>
               <destarmlen>0.5</destarmlen>
               <label>
                     <name><None/></name>
               </label>
               <inverse>
                  <label>
                     <position>
                        <Right/>
                     </position>
                  </label>
               </inverse>
            </path>
         </diagram>
      </composition>
   </entity_type>
   <group>
      <name>city and city </name>
      <presentation>
         <below>state</below>
         <y>0.9</y>
      </presentation>
      <entity_type>
         <name>city</name>
         <presentation>
            <y>0</y>
         </presentation>
         <dependency>
            <name>located_within</name>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <type>state</type>
            <inverseOf>location_of</inverseOf>
         </dependency>
         <composition>
            <name>start_of</name>
            <type>route</type>
            <cardinality>
               <ZeroOneOrMore/>
            </cardinality>
            <identifying/>
            <inverse>from</inverse>         
            <id>d2</id>
            <diagram>
               <path>
                  <destattach>0.3</destattach>
                  <destarmlen>0.6</destarmlen>
                  <label>
                        <name><None/></name>
                  </label>
                  <inverse>
                     <label>
                        <position>
                           <Left/>
                        </position>
                     </label>
                  </inverse>
               </path>
            </diagram>
         </composition>
      </entity_type>
      <entity_type>
         <name>city </name>
         <dependency>
            <name>located_within</name>
            <cardinality>
               <ExactlyOne/>
            </cardinality>
            <type>state</type>
            <inverseOf>location_of </inverseOf>
         </dependency>
               <composition>
            <name>end_of</name>
            <cardinality>
               <ZeroOneOrMore/>
            </cardinality>
            <identifying/>
            <type>route</type>
            <inverse>to</inverse>
               <id>d3</id>
            <diagram>
               <path>
                  <destattach>0.7</destattach>
                  <destarmlen>0.6</destarmlen>
                  <label>
                        <name><None/></name>
                  </label>
               </path>
            </diagram>
         </composition>
         <presentation>
            <rightOf>city</rightOf>
            <x>0.8</x>
         </presentation>
      </entity_type>
   </group>
   <entity_type>
      <name>route</name>
      <presentation>
         <below>city and city </below>
         <y>1.7</y>
      </presentation>
      <dependency>
         <name>from</name>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <type>city</type>
         <inverseOf>start_of</inverseOf>
      </dependency>
      <dependency>
         <name>to</name>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <type>city </type>
         <inverseOf>end_of</inverseOf>
      </dependency>
   </entity_type>
</entity_model>
