      <reference>
         <name>portrayed_by</name>
         <type>character</type>
         <cardinality>
            <ZeroOneOrMore/>
         </cardinality>
         <inverse>the_portrayal_of</inverse>
      </reference>
   </entity_type>

   <entity_type>
      <name>production</name>
      <presentation>
         <rightOf>character</rightOf>
         <x>1.7</x>
         <y>-2.5</y>
         <h>0.9</h>
      </presentation>
      <!-- <attribute><name>season</name><type><string/></type><identifying/></attribute> -->
      <composition>
         <name>cast_with</name>
         <type>dramatic_role</type>
         <cardinality>
            <OneOrMore/>
         </cardinality>
         <inverse>in</inverse>
         <diagram>
            <path>
               <align><ToDest/></align>
               <label>
                  <position><Left/></position>
               </label>
            </path>
         </diagram>
      </composition>
   </entity_type>

   <entity_type>
      <name>dramatic_role</name>
      <presentation>
         <below>production</below>
         <rightOf>character</rightOf>
         <x>0.2</x>
         <h>0.9</h>
         <name>
            <Split/>
         </name>
      </presentation>
      <dependency>
         <name>in</name>
         <type>production</type>
         <inverse>cast_with</inverse>
         <identifying/>
      </dependency>
      <reference>
         <name>the_portrayal_of</name>
         <type>character</type>
         <cardinality>
            <ExactlyOne/>
         </cardinality>
         <inverse>portrayed_by</inverse>
         <identifying/>
         <diagram>
            <path>
               <align><ToDest/></align>
            </path>
         </diagram>
         <diagram>
            <path>
               <label>
                  <position>
                     <Upside/>
                  </position>
               </label>
               <inverse>
                  <label>
                     <position>
                        <Downside/>
                     </position>
                  </label>
               </inverse>
               <scope>
                  <label>
                     <xAdjustment>-0.5</xAdjustment>
                  </label>
               </scope>
            </path>
         </diagram>
      </reference>
   </entity_type>
